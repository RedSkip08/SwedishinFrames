        "meaning_en": "a school mark showing how well someone did in a subject or course",
        "how_to_use_en": [
          "Use for school grades (A–F, pass/fail, etc.).",
          "Often used with adjectives like bra/dåliga and with a subject: betyg i + ämne."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "få [ADJ] betyg",
            "meaning_en": "get (good/bad) grades",
            "note_en": "Common with bra/dåliga."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ge [NÅGON] [ADJ] betyg",
            "meaning_en": "give someone grades",
            "note_en": "Used when a teacher grades a student."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "betyg i + ämne",
            "meaning_en": "grade in a subject",
            "note_en": "Often used when talking about a specific subject."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "utan betyg",
            "meaning_en": "without grades",
            "note_en": "Used when someone lacks grades in a subject."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "baserat på betyg",
            "meaning_en": "based on grades",
            "note_en": "Common in admission/selection contexts."
          }
        ],
        "common_errors_en": [
