    "senses": [
      {
        "sense_id": "lu_unterrichtsstunde_die:1",
        "frame_id": "frame_education",
        "meaning_en": "a teaching session where someone learns a subject, fact, or skill",
        "how_to_use_en": [
          "Use for one class session: en lektion.",
          "Often with numbers (lektion 1, lektion 2) or order (första, tredje).",
          "Often with i + subject: en lektion i ryska."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "i (sin) första/tredje lektion",
            "meaning_en": "in someone’s first/third lesson",
            "note_en": "Used to place learning or progress in a sequence of lessons."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "i lektion + siffra",
            "meaning_en": "in lesson number X",
            "note_en": "Common in textbooks/manuals: i lektion 1, i lektion 2."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "en lektion i + ämne",
            "meaning_en": "a lesson in a subject",
            "note_en": "Names what the lesson is about (e.g., ryska)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "närvara på en lektion",
            "meaning_en": "attend a lesson",
            "note_en": ""
          },
          {
            "pattern_id": "pat_5",
            "pattern": "gå (flera timmar) på lektion",
            "meaning_en": "do/attend lessons",
            "note_en": "Here på lektion means being in lessons as an activity."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *kurs* when you mean one meeting.",
            "fix": "Use lektion for one session; use kurs for a longer course.",
            "explain_en": "In the examples, lektion refers to a single lesson (often numbered or ‘first/third’)."
          },
          {
            "error": "Saying *på en lektion i ryska* when you mean the topic.",
            "fix": "Say en lektion i ryska; use på lektion when you mean ‘during lessons’.",
            "explain_en": "The examples show en lektion i + subject, and also gå ... på lektion for being in lessons."
          }
        ],
