    "senses": [
      {
        "sense_id": "lu_unterrichtsstunde_die:1",
        "frame_id": "frame_classes_&_courses",
        "meaning_en": "one teaching session in a course, often numbered or about a specific subject",
        "how_to_use_en": [
          "Use for one class meeting: en lektion.",
          "Often with a number (lektion 1, lektion 2) or with a subject (lektion i ryska).",
          "Use with prepositions like i, under, på depending on context."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "i lektion + siffra",
            "meaning_en": "in lesson (number)",
            "note_en": "Common in textbooks or course units: i lektion 1, i lektion 2."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "i/på/under en lektion",
            "meaning_en": "during a lesson",
            "note_en": "Choice of preposition depends on meaning: under = during; på = attending/being in lessons."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "en lektion i + ämne",
            "meaning_en": "a lesson in (subject)",
            "note_en": "Used for the topic taught: en lektion i ryska."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "sin första/tredje lektion",
            "meaning_en": "one’s first/third lesson",
            "note_en": "Often used when someone is learning a skill over several lessons."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *klass* when you mean one scheduled teaching session.",
            "fix": "Use lektion for one session; use klass/klassrum for the group/room.",
            "explain_en": "Lektion focuses on the session/period, not the group of students."
          },
          {
            "error": "Saying *på lektion 1* for numbered lessons in texts.",
            "fix": "Prefer i lektion 1 / i lektion 2.",
            "explain_en": "The examples show i + lektion + number for lesson units/chapters."
          }
        ],
