    "senses": [
      {
        "sense_id": "lu_avsluta_das_studium_avsluta:1",
        "frame_id": "frame_education",
        "meaning_en": "finish and complete a period of study or an education program.",
        "how_to_use_en": [
          "Use with a direct object: avsluta + studies/education/school.",
          "Common in formal or neutral writing about education.",
          "Often used with purpose: för att avsluta ..."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "avsluta + studierna / sina studier",
            "meaning_en": "finish one’s studies",
            "note_en": "Very common with definite form: studierna."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "avsluta + (sin) utbildning",
            "meaning_en": "finish an education program",
            "note_en": "Often used for professional training (e.g., medicinsk utbildning)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "avsluta + skola (t.ex. elementärskolan, high school)",
            "meaning_en": "finish school",
            "note_en": "Used for completing a school level."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "för att avsluta + studier/utbildning",
            "meaning_en": "in order to finish studies",
            "note_en": "Purpose clause explaining why someone goes somewhere/does something."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "utan att avsluta + studierna",
            "meaning_en": "without finishing the studies",
            "note_en": "Shows that the studies were not completed."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *avsluta* without saying what is finished.",
            "fix": "Add the object: avsluta studierna/utbildningen/skolan.",
            "explain_en": "In this education sense, *avsluta* normally needs an object."
          },
          {
            "error": "Confusing *avsluta* with *sluta*.",
            "fix": "Use *avsluta* for completing; use *sluta* for stopping/quitting.",
            "explain_en": "*Avsluta* focuses on finishing something; *sluta* often means to stop or quit."
          }
        ],
