      {
        "sense_id": "lu_freund_der:1",
        "frame_id": "frame_dating",
        "meaning_en": "a male romantic partner; the man someone is dating or together with",
        "how_to_use_en": [
          "Use for a man in a romantic relationship (not necessarily married).",
          "Often used with a possessive: min/sin/hennes/hans pojkvän.",
          "Can be used with “en” when the person is not specified: en pojkvän."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X och hennes/hans pojkvän",
            "note_en": "Two people mentioned together; the boyfriend belongs to X."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "hennes/hans/sin pojkvän",
            "note_en": "Very common possessive pattern."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "en (ny) pojkvän",
            "note_en": "Indefinite form, often with an adjective like ny."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "ses tillsammans med en pojkvän",
            "note_en": "Used when someone is seen together with a boyfriend."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "gifta sig med sin pojkvän",
            "note_en": "A relationship stage where the boyfriend becomes a husband."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *pojkvän* for a male friend (not romantic).",
            "fix": "Use vän / kompis for a non-romantic friend.",
            "explain_en": "Pojkvän normally means a romantic partner, not just a friend."
          },
          {
            "error": "Mixing up sin/hans/hennes with possessives.",
            "fix": "Use sin when it refers back to the subject; otherwise use hans/hennes.",
            "explain_en": "In the examples: hon + sin pojkvän; Mary + hennes pojkvän."
          }
