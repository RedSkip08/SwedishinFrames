      {
        "sense_id": "lu_freund_der:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "a person you like and know well; not a family member",
        "how_to_use_en": [
          "Use for a personal relationship: someone is another person’s friend.",
          "Often used with possessives: hans/sin/hennes + vän.",
          "Use bli vän med + person to talk about becoming friends."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X:s vän",
            "meaning_en": "X’s friend",
            "note_en": "Use genitive -s to show whose friend it is."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "hans/hennes/sin vän + Namn",
            "meaning_en": "his/her/own friend + name",
            "note_en": "Often followed by the friend’s name as extra information."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bli vän med + person",
            "meaning_en": "become friends with someone",
            "note_en": "Talks about starting a friendship."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "bästa/bäste vän",
            "meaning_en": "best friend",
            "note_en": "Common fixed phrase for a very close friend."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *vän* to mean romantic partner.",
            "fix": "Use partner, pojkvän/flickvän, or make it clear with context.",
            "explain_en": "In Swedish, vän usually means a non-romantic friend."
          },
          {
            "error": "Saying *bli vän till* someone.",
            "fix": "Say bli vän med + someone.",
            "explain_en": "The examples show the fixed preposition med."
          }
