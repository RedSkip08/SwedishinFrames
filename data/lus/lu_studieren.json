        "sense_id": "lu_studieren:1",
        "frame_id": "frame_learning_&_studying",
        "meaning_en": "learn a subject, usually in an organized way, often at a school or university",
        "how_to_use_en": [
          "Use for studying a subject: studera + ämne.",
          "Use for studying at an institution: studera vid/på + skola/universitet.",
          "Often used with börja/började to talk about starting studies."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "börja(de) studera + Subject",
            "meaning_en": "start studying a subject",
            "note_en": "Often followed by the subject name, e.g. matematik, medicin."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "börja(de) studera vid + Institution",
            "meaning_en": "start studying at an institution",
            "note_en": "Common for universities and schools."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "studera vid + Institution",
            "meaning_en": "study at an institution",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "studera + Subject",
            "meaning_en": "study a subject/topic",
            "note_en": "Can be a school subject or a broader topic/period."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "studera under eget ansvar",
            "meaning_en": "study independently",
            "note_en": "Describes the method: the learner studies on their own responsibility."
          }
        ],
