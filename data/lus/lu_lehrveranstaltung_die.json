    "senses": [
      {
        "sense_id": "lu_lehrveranstaltung_die:1",
        "frame_id": "frame_classes_&_courses",
        "meaning_en": "an organized set of lessons to learn a subject, often at a school or organization",
        "how_to_use_en": [
          "Use for a course someone takes or a school offers.",
          "Often followed by i + subject (kurs i X) or at an institution (kurs vid/på X).",
          "Common with verbs like gå, anmäla sig till, påbörja."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "gå en kurs",
            "meaning_en": "take a course",
            "note_en": "Very common with learners/participants."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "en kurs i + ämne",
            "meaning_en": "a course in a subject",
            "note_en": "Used for the topic/field (e.g., HLR, japanska, skådespeleri)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "en kurs genom + institution",
            "meaning_en": "a course through an organization",
            "note_en": "Highlights who provides/arranges the course."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "en kurs vid + institution",
            "meaning_en": "a course at an institution",
            "note_en": "Common with schools, studios, etc."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "anmäla sig till en kurs",
            "meaning_en": "sign up for a course",
            "note_en": "Focuses on registration."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *klass* when you mean a course/program unit.",
            "fix": "Use kurs for the organized teaching unit; klass is usually a school group or a single class.",
            "explain_en": "In these examples, kurs is the unit you take (e.g., a CPR course), not the group of students."
          },
          {
            "error": "Forgetting the plural form *kurser*.",
            "fix": "Say kurser (indefinite plural) and kurserna (definite plural).",
            "explain_en": "The plural changes vowel and adds -er."
          }
        ],
