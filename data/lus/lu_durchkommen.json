        "common_errors_en": [
          {
            "error": "Using *klara* without saying what is handled (when context is unclear).",
            "fix": "Add the thing: “klara hettan/tyngden/kraven/sjukdomen”.",
            "explain_en": "In this sense, *klara* usually needs an object (the challenge or requirement)."
