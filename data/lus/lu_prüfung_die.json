      {
        "sense_id": "lu_prüfung_die:1",
        "frame_id": "frame_education",
        "meaning_en": "a test or exam used to check knowledge or skills",
        "how_to_use_en": [
          "Use for school or training tests (written, practical, national).",
          "Often with a subject: prov i + school subject.",
          "Also used with verbs like göra/ha/avlägga + prov."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "göra prov (inom/i + ämne)",
            "meaning_en": "take a test in a subject",
            "note_en": "The example shows inom with a list of subjects."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ha (ett) prov",
            "meaning_en": "have a test (scheduled)",
            "note_en": "Often used about a test happening on a certain day/week."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "nationella prov (i + årskurs/kurs)",
            "meaning_en": "national tests in a grade/course",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "avlägga prov",
            "meaning_en": "take a formal test",
            "note_en": "More formal than göra prov."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "prov på + färdighet/egenskap",
            "meaning_en": "test of a skill/quality",
            "note_en": "Used when the test measures abilities rather than a school subject."
          }
        ],
        "common_errors_en": [
