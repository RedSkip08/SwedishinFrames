    "senses": [
      {
        "sense_id": "lu_freundschaft_die:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "a friendly relationship between people that develops and can last over time",
        "how_to_use_en": [
          "Use it for a relationship between two people or a group.",
          "Common with verbs like uppstå, växa fram, utveckla, återställa, vara, fortsätta."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "vänskap uppstår",
            "meaning_en": "friendship begins",
            "note_en": "Used when a friendship starts, often after an event."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "en nära vänskap (med [person])",
            "meaning_en": "a close friendship",
            "note_en": "Often followed by med + person."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "en vänskap växer fram (mellan dem)",
            "meaning_en": "friendship gradually develops",
            "note_en": "Emphasizes a slow development over time."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "återställa deras vänskap",
            "meaning_en": "restore their friendship",
            "note_en": "Used when a friendship has been damaged."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "deras / X och Y:s vänskap varade / fortsatte",
            "meaning_en": "their friendship lasted/continued",
            "note_en": "Common with time expressions (e.g., i 60 år)."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *vänskap* when you mean one person (a friend).",
            "fix": "Use *en vän* for a person; use *vänskap* for the relationship.",
            "explain_en": "Vänskap is the bond/relationship, not the individual."
          },
          {
            "error": "Forgetting the possessive when talking about a specific relationship (e.g., *vänskap varade*).",
            "fix": "Use a possessive: *deras vänskap*, *X och Y:s vänskap*.",
            "explain_en": "In the examples, specific friendships are often marked with deras or a genitive."
          }
        ],
