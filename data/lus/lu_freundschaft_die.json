    "senses": [
      {
        "sense_id": "lu_freundschaft_die:1",
        "frame_id": "frame_friendship",
        "meaning_en": "a friendly relationship between people who like and support each other",
        "how_to_use_en": [
          "Use it for the relationship/bond between two people or a group.",
          "Common with adjectives like nära to show how close it is.",
          "Often used with verbs like uppstå, växa fram, utveckla, var(a)da, fortsätta, återställa, vinna."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "vänskap uppstår",
            "meaning_en": "friendship begins",
            "note_en": "Used when a bond starts to exist."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "en vänskap växer fram (mellan X och Y)",
            "meaning_en": "friendship gradually develops",
            "note_en": "Often with time expressions like så småningom, med tiden."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "ha en (nära) vänskap med X",
            "meaning_en": "have a (close) friendship with",
            "note_en": "Use nära to show a strong bond."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "vänskapen varade i + tid",
            "meaning_en": "friendship lasted for time",
            "note_en": "Use i for duration (e.g., i 60 år)."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "återställa deras vänskap",
            "meaning_en": "restore their friendship",
            "note_en": "Used when the relationship has been damaged."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *vänskap* when you mean the person (a friend).",
            "fix": "Use vän (singular) or vänner (plural) for the people.",
            "explain_en": "Vänskap is the relationship; vän is a person."
          },
          {
            "error": "Saying *vänskap till någon* in this sense.",
            "fix": "Say vänskap med någon or vänskap mellan (två personer).",
            "explain_en": "The examples show med and mellan for the relationship."
          }
        ],
