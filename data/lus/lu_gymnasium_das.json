    "senses": [
      {
        "sense_id": "lu_gymnasium_das:1",
        "frame_id": "frame_schools",
        "meaning_en": "a school level after compulsory school, usually for teenagers, preparing for work or further studies",
        "how_to_use_en": [
          "Use for the school level after grundskolan (upper secondary).",
          "Often used with gå i (“attend”) or after (“after finishing”).",
          "Can also name a specific school: X gymnasium."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "gå i gymnasium (i + plats/ort)",
            "meaning_en": "attend upper secondary school",
            "note_en": "Often followed by a location: i Solothurn."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "efter gymnasium",
            "meaning_en": "after upper secondary school",
            "note_en": "Used when describing what happens next in someone’s education."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X gymnasium är en skola i Y",
            "meaning_en": "X is an upper secondary school in Y",
            "note_en": "Common in descriptions of specific schools."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "ha ett gymnasium (med + utbildning/program)",
            "meaning_en": "have an upper secondary school (with a programme)",
            "note_en": "Used about a town/municipality offering a school and programmes."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "undervisa i ... och gymnasium",
            "meaning_en": "teach in ... and upper secondary",
            "note_en": "Lists school levels where someone teaches."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using gymnasium to mean a sports hall (English “gym”).",
            "fix": "Use gympasal or idrottshall for the sports hall.",
            "explain_en": "Swedish gymnasium usually means upper secondary school, not a place to work out."
          },
          {
            "error": "Saying *i gymnasiet* when you mean the level in general.",
            "fix": "Often use i gymnasium (general) or på gymnasiet (at the school).",
            "explain_en": "Both occur, but i gymnasium is common for the level; on/på is common for the institution."
          }
        ],
