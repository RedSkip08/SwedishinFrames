        "sense_id": "lu_gymnasium_das:1",
        "frame_id": "frame_education",
        "meaning_en": "a school for upper secondary education, after compulsory school.",
        "how_to_use_en": [
          "Use for the school level (upper secondary) or a specific school name.",
          "Often appears in names: X gymnasium.",
          "Common after prepositions like i and efter."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X gymnasium är en skola ...",
            "meaning_en": "X is an upper secondary school",
            "note_en": "Common in descriptions of specific schools."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "i gymnasium",
            "meaning_en": "in upper secondary school",
            "note_en": "Used with gå i (attend) or undervisa i (teach in)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "efter gymnasium",
            "meaning_en": "after upper secondary school",
            "note_en": "Used when describing what someone did next in education or life."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "grundskolan och gymnasium",
            "meaning_en": "primary school and upper secondary school",
            "note_en": "Often used to list school levels."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "med gymnasium, fackhögskola och ...",
            "meaning_en": "with a gymnasium (as an institution)",
            "note_en": "Used when listing institutions a place has."
          }
        ],
