    "senses": [
      {
        "sense_id": "lu_professor_der_professor__die_professorin:1",
        "frame_id": "frame_education",
        "meaning_en": "a senior university teacher and researcher, often responsible for a subject area",
        "how_to_use_en": [
          "Use for a person’s academic title at a university.",
          "Often followed by i + subject and/or vid + university."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "vara professor i + ÄMNE",
            "meaning_en": "be a professor of a subject",
            "note_en": "Used to state the academic field."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "vara professor vid + INSTITUTION",
            "meaning_en": "be a professor at an institution",
            "note_en": "Common with universities and faculties."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "vara professor i + ÄMNE + vid + INSTITUTION",
            "meaning_en": "be a professor of X at Y",
            "note_en": "Combines field and workplace."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "bli professor (där) + ÅRTAL",
            "meaning_en": "become a professor (there) + year",
            "note_en": "Often used in biographies."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using professor for any school teacher.",
            "fix": "Use lärare for general teacher; use professor for university title.",
            "explain_en": "In Swedish, professor is mainly a university title, as in the examples."
          },
          {
            "error": "Forgetting the preposition in subject/institution phrases.",
            "fix": "Say professor i + ämne, professor vid + universitet.",
            "explain_en": "The examples show i for the subject and vid for the institution."
          }
        ],
