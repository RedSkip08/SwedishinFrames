    "senses": [
      {
        "sense_id": "lu_professor_der_professor__die_professorin:1",
        "frame_id": "frame_people",
        "meaning_en": "a university teacher and researcher with the title professor",
        "how_to_use_en": [
          "Use for a person’s academic title at a university.",
          "Often followed by i + subject area and/or vid + university."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "vara professor i + [ämne]",
            "meaning_en": "be a professor of a subject",
            "note_en": "Used to name the academic field."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "vara professor vid + [universitet/institution]",
            "meaning_en": "be a professor at an institution",
            "note_en": "Vid introduces the university or faculty."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bli professor (där) + [årtal]",
            "meaning_en": "become a professor (there)",
            "note_en": "Often used in biographies with a year."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "idag professor vid + [universitet]",
            "meaning_en": "today a professor at",
            "note_en": "Common in short descriptions of a person."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "tillförordnad professor vid + [institution]",
            "meaning_en": "acting professor at",
            "note_en": "Tillförordnad means temporary/acting."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *på* for the institution in this sense (e.g., *professor på universitetet*).",
            "fix": "Use vid for the institution (e.g., professor vid Uppsala universitet).",
            "explain_en": "In the examples, the institution is introduced with vid."
          },
          {
            "error": "Forgetting i before the subject area (e.g., *professor juridik*).",
            "fix": "Use professor i + field (e.g., professor i juridik).",
            "explain_en": "The field is expressed with i in the examples."
          }
        ],
