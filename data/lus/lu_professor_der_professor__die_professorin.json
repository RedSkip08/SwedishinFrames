    "senses": [
      {
        "sense_id": "lu_professor_der_professor__die_professorin:1",
        "frame_id": "frame_education",
        "meaning_en": "a person who teaches and works as a professor at a university",
        "how_to_use_en": [
          "Use for a university teacher with the title professor.",
          "Often followed by the subject with i and the institution with vid."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "vara professor i + Ämne",
            "meaning_en": "be professor of a subject",
            "note_en": "Used to state the academic field."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "vara professor vid + Institution",
            "meaning_en": "be professor at an institution",
            "note_en": "Common in biographies and CV-style text."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bli professor (där) + Årtal",
            "meaning_en": "become a professor",
            "note_en": "Often with a year and sometimes där referring to the institution."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "(idag) professor vid + Institution",
            "meaning_en": "currently a professor at",
            "note_en": "Idag marks the current situation."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using professor for any school teacher.",
            "fix": "Use lärare for general teachers; use professor mainly at universities.",
            "explain_en": "In Swedish, professor is a specific academic title, not a general word for teacher."
          },
          {
            "error": "Saying *professor på universitetet* in this sense.",
            "fix": "Say professor vid [universitet].",
            "explain_en": "The examples use vid to mark the institution."
          }
