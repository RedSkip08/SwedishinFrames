        "frame_id": "frame_personal_relationship",
        "meaning_en": "to be a couple; to be in a romantic relationship with someone",
        "how_to_use_en": [
          "Use about two people who are a couple.",
          "Often with a time expression (e.g., länge, alltid, i 12 år).",
          "Add med + person to name the partner."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X vara tillsammans",
            "meaning_en": "X are a couple",
            "note_en": "X is usually two people (de, paret, Romeo och Julia)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X vara tillsammans med Y",
            "meaning_en": "X is in a relationship with Y",
            "note_en": "Used when you name the partner (med Bonnie eller Rachel)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "inte (ska/vill) vara tillsammans (längre)",
            "meaning_en": "not be together anymore",
            "note_en": "Common when ending a relationship."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "alltid/få vara tillsammans",
            "meaning_en": "be together always",
            "note_en": "Often expresses a wish or promise."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "vara tillsammans i + tid",
            "meaning_en": "be together for a time",
            "note_en": "Used with durations like i 12 år."
          }
        ],
        "common_errors_en": [
