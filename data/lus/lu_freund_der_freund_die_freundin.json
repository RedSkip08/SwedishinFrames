        "how_to_use_en": [
          "Use for a male partner in a romantic relationship.",
          "Often used with possessives: min/sin/hennes/hans pojkvän.",
          "Can be used with en: en pojkvän."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "hennes/hans + pojkvän",
            "meaning_en": "someone’s boyfriend",
            "note_en": "Used when the boyfriend belongs to another person mentioned."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "sin + pojkvän",
            "meaning_en": "their/one’s own boyfriend",
            "note_en": "Sin refers back to the subject (e.g., hon ... sin pojkvän)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "en pojkvän",
            "meaning_en": "a boyfriend",
            "note_en": "Indefinite form when introducing a boyfriend."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "ny/nye + pojkvän",
            "meaning_en": "new boyfriend",
            "note_en": "Common when someone has started a new relationship."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "flick- eller pojkvän",
            "meaning_en": "girlfriend or boyfriend",
            "note_en": "Used to include both possibilities."
          }
        ],
