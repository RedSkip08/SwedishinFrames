        "meaning_en": "a mark showing how well someone did in a school subject or course",
        "how_to_use_en": [
          "Use for school/university results (a mark/grade).",
          "Often used in plural when talking about someone’s overall results.",
          "Common with adjectives like bra/dåliga and with a subject: betyg i + ämne."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "få (ADJ) betyg",
            "meaning_en": "get (good/bad) grades",
            "note_en": "Used about a student’s results."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ge någon (ADJ) betyg",
            "meaning_en": "give someone grades",
            "note_en": "Typically a teacher gives grades."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "betyg i + ämne",
            "meaning_en": "grade in a subject",
            "note_en": "Names the subject area."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "utan betyg",
            "meaning_en": "without grades",
            "note_en": "Used when someone lacks grades in a subject."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "baserat på / på grundval av betyg",
            "meaning_en": "based on grades",
            "note_en": "Common in admissions/selection contexts."
          }
        ],
        "common_errors_en": [
