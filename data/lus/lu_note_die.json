        "meaning_en": "a grade or rating given for school work, a subject, or a test result",
        "how_to_use_en": [
          "Use for school grades (e.g., in a subject) and for ratings/scores (e.g., stars, points).",
          "Often used with verbs like få (get), ge (give), sätta (set), and with adjectives like bra/dåliga."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "få + (ADJ) + betyg",
            "meaning_en": "get (good/bad) grades",
            "note_en": "Often about school performance."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "få + [bokstav/poäng] + i betyg",
            "meaning_en": "get a letter grade",
            "note_en": "Used with systems like A, B, E, F."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "ge + [person] + (ADJ) + betyg",
            "meaning_en": "give someone grades",
            "note_en": "Typically a teacher gives grades."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "sätta betyg",
            "meaning_en": "set grades",
            "note_en": "About officially deciding grades."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "ett betyg på + [tal]",
            "meaning_en": "a score of (number)",
            "note_en": "Common for ratings like 74/100."
          }
