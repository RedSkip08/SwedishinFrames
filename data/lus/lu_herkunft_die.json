      {
        "sense_id": "lu_herkunft_die:1",
        "frame_id": "frame_origin",
        "meaning_en": "where something or someone comes from; the starting source or background.",
        "how_to_use_en": [
          "Use to talk about where something comes from (place, culture, cause, or history).",
          "Common with “av … ursprung” and “ha sitt ursprung i …”.",
          "Often used in factual descriptions (e.g., food, words, families, phenomena)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är av + (adjektiv) + ursprung",
            "meaning_en": "X is of … origin",
            "note_en": "Often describes type/source (e.g., volcanic, Chinese, English)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X har (ett) + (adjektiv) + ursprung",
            "meaning_en": "X has a … origin",
            "note_en": "Common with adjectives like oklart/okänt/gemensamt."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X har sitt ursprung i + plats/orsak",
            "meaning_en": "X originates in/from …",
            "note_en": "Used for places and also causes (e.g., “i okunskap …”)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X är … med ursprung i + plats",
            "meaning_en": "X is … with origin in …",
            "note_en": "Often used for names, families, or groups."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *ursprung* for a person’s birthplace when you mean the exact place they were born.",
            "fix": "Use “födelseort” for birthplace; use “ursprung” for background/origin.",
            "explain_en": "Ursprung is broader (background/source), while födelseort is the specific birth place."
          },
          {
            "error": "Saying *i ursprung* when you mean “of … origin”.",
            "fix": "Say “av + adjective + ursprung” (e.g., “av vulkaniskt ursprung”).",
            "explain_en": "The examples show the fixed pattern “av … ursprung” for type/source."
          }
        ],
