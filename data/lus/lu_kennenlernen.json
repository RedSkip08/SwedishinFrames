    "senses": [
      {
        "sense_id": "lu_kennenlernen:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "to start knowing someone or something better through time and experience",
        "how_to_use_en": [
          "Use for getting familiar with a person, group, or culture over time.",
          "Often used with an object (a person/thing) or with varandra for “each other”.",
          "Common with words like äntligen, möjlighet, försöka, bättre."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X lär känna Y",
            "meaning_en": "X gets to know Y",
            "note_en": "Y is typically a person or group."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "lära känna varandra (bättre)",
            "meaning_en": "get to know each other (better)",
            "note_en": "Used when the relationship is mutual."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "få möjlighet att lära känna Y",
            "meaning_en": "get the chance to know Y",
            "note_en": "Common in school/work introductions."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "komma att lära känna Y",
            "meaning_en": "come to get to know Y",
            "note_en": "Often about something that happens over time."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "försöka lära känna Y",
            "meaning_en": "try to get to know Y",
            "note_en": "Often used when it takes effort."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using känna for “get to know” (e.g., *känna henne).",
            "fix": "Use lära känna henne.",
            "explain_en": "känna usually means you already know someone; lära känna is the process of getting to know."
          },
          {
            "error": "Forgetting varandra when the meaning is mutual.",
            "fix": "Use lära känna varandra.",
            "explain_en": "When two or more people get to know each other, Swedish normally uses varandra."
          }
