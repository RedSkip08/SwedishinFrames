      {
        "sense_id": "lu_kumpel_der:1",
        "frame_id": "frame_friendship",
        "meaning_en": "a person you know well and like, not a family member",
        "how_to_use_en": [
          "Use for a friend in everyday, informal Swedish.",
          "Often used with a possessive: hans/hennes/sin kompis + name.",
          "Also used in the expression bli kompis (med) = become friends (with)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "en kompis",
            "meaning_en": "a friend",
            "note_en": "Indefinite singular, often when introducing someone."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "hans/hennes/sin kompis + Namn",
            "meaning_en": "someone’s friend (named)",
            "note_en": "Used to identify a friend: e.g., hans kompis Pontus."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "av/från sin kompis + Namn",
            "meaning_en": "from one’s friend",
            "note_en": "Common with verbs like få reda på (find out)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "mot en kompis",
            "meaning_en": "against a friend",
            "note_en": "Often in games or competitions."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "bli kompis (med) + person",
            "meaning_en": "become friends (with)",
            "note_en": "Fixed expression meaning to start a friendship."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using kompis for a romantic partner in neutral contexts.",
            "fix": "Use partner, pojkvän/flickvän, or make the relationship clear.",
            "explain_en": "Kompis normally means a friend; it does not usually imply a romantic relationship."
          },
          {
            "error": "Forgetting the plural ending: *två kompis.",
            "fix": "Say två kompisar.",
