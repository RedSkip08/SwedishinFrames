      {
        "sense_id": "lu_verlobte_der_verlobte_ein_verlobter__die_eine_verlobte:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "a man you are engaged to marry",
        "how_to_use_en": [
          "Use for a male partner when a couple is engaged.",
          "Often with a possessive: hennes/sin/min fästman.",
          "Can be followed by a name: hennes fästman Kurre."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X:s fästman (Namn)",
            "note_en": "Possessive/genitive + fästman, often with a name. Example: Annas fästman Sven."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "hennes fästman (Namn)",
            "note_en": "With hennes + fästman, optionally followed by a name. Example: hennes fästman Ole."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "sin fästman (Namn)",
            "note_en": "Reflexive possessive sin when the subject is the same person. Example: Sophie … sin fästman Sky."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "ingen fästman",
            "note_en": "Negation with ingen. Example: Hon har ingen fästman."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "berätta om sin fästman",
            "note_en": "Common with verbs like berätta/skriva (brev) + till/om. Examples: berätta om sin fästman; skriva … till sin fästman."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using fästman for a boyfriend you are not engaged to.",
            "fix": "Use pojkvän (or partner) if you are not engaged.",
            "explain_en": "fästman implies an engagement (a plan to marry)."
          },
          {
            "error": "Wrong plural: *fästmaner.",
            "fix": "Say fästmän.",
            "explain_en": "The plural is irregular: fästman → fästmän."
          }
        ],
