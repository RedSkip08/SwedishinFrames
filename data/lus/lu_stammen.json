      {
        "sense_id": "lu_stammen:1",
        "frame_id": "frame_origin",
        "meaning_en": "to come from an earlier source, place, group, or time; to have an origin",
        "how_to_use_en": [
          "Use it to state where something comes from: härstamma från + origin.",
          "Often used in factual or formal writing (history, science).",
          "Can also be used about time: härstamma från + year/century."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X härstammar från Y",
            "meaning_en": "X originates from Y",
            "note_en": "Common for animals, objects, diseases, and traditions."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X anses/tros/lär/sägs (sig) härstamma från Y",
            "meaning_en": "X is believed to originate from Y",
            "note_en": "Often used when the origin is uncertain or reported."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X härstammar från + tid (år/århundrade)",
            "meaning_en": "X dates from a time",
            "note_en": "Used for buildings, parts, ships, etc."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *härstamma* without från (e.g., *härstamma Indien*).",
            "fix": "Use från: härstamma från Indien.",
            "explain_en": "In the examples, the origin is introduced with från."
          },
          {
            "error": "Using it for a person’s current location (meaning 'come from' as in travel).",
            "fix": "Use komma från for simple everyday origin; use härstamma for deeper origin/roots.",
            "explain_en": "Härstamma is typically about origin/roots, not where someone just arrived from."
          }
        ],
