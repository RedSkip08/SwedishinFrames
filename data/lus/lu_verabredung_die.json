          },
          {
            "pattern_id": "pat_2",
            "pattern": "komma till deras/dejten",
            "meaning_en": "show up for the date",
            "note_en": "Often used when someone cannot come or does not come."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bjuda ut någon på en dejt",
            "meaning_en": "ask someone out on a date",
            "note_en": ""
