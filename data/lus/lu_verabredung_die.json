    "senses": [
      {
        "sense_id": "lu_verabredung_die:1",
        "frame_id": "frame_dating",
        "meaning_en": "a planned romantic meeting, or the person you go with to an event",
        "how_to_use_en": [
          "Use for a romantic meeting: på en dejt, deras dejt.",
          "Use for a person you bring with you: vara någons dejt, ta med en dejt.",
          "Often with an ordinal: första/andra/tredje dejt."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "vara någons dejt (på + event)",
            "note_en": "A person as someone’s companion at an event (e.g., studentbalen)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "komma till + (deras) dejt",
            "note_en": "A planned meeting you can come to or miss."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bjuda ut + någon + på en dejt",
            "note_en": "Invite/ask someone out on a date."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "gå på + (sin/deras) + (första/andra/tredje) + dejt",
            "note_en": "Talk about the stage/number of dates."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "ta med + en dejt",
            "note_en": "Bring a date (a person) to an event."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *dejt* for any meeting (e.g., a work meeting).",
            "fix": "Use möte/träff for non-romantic meetings.",
            "explain_en": "In these examples, *dejt* is connected to dating/romance or being someone’s companion at an event."
          },
          {
            "error": "Wrong preposition: *i en dejt*.",
            "fix": "Say på en dejt / till en dejt / komma till deras dejt.",
            "explain_en": "The examples show on/to: 'på en dejt', 'komma till deras dejt'."
          }
        ],
