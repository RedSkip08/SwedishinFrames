      {
        "sense_id": "lu_schuljahr_das:1",
        "frame_id": "frame_education",
        "meaning_en": "a year-long period of study at school, often counted as year 1, 2, etc.",
        "how_to_use_en": [
          "Use for the school year as a unit of time in education.",
          "Often used with numbers (skolår 8) or with adjectives (ett nytt skolår).",
          "Can be used in plural for several years (nio eller tio skolår)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "ett/ett nytt + skolår",
            "meaning_en": "a (new) school year",
            "note_en": "Used when a school year begins."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "mellan två + skolår",
            "meaning_en": "between two school years",
            "note_en": "Often about holidays between years."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "i skolår + tal (t.ex. 8)",
            "meaning_en": "in school year (number)",
            "note_en": "Refers to a specific year level."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "i nio/tio + skolår",
            "meaning_en": "for nine/ten school years",
            "note_en": "Used to state duration in years."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "per skolår",
            "meaning_en": "per school year",
            "note_en": "Common with fees or costs."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using en: *en skolår",
            "fix": "ett skolår",
            "explain_en": "skolår is an ett-word (neuter)."
          },
          {
            "error": "Confusing skolår with termin",
            "fix": "Use skolår for the whole year; termin for a half-year term.",
            "explain_en": "A skolår typically includes two terminer."
          }
