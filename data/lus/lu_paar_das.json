        "sense_id": "lu_paar_das:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "two things that belong together, or a small number (about two).",
        "how_to_use_en": [
          "Use ett par + plural noun for ‘a couple of’ (ett par år, ett par saker).",
          "Use it for two matching items (ett par byxor, ett par ljusstakar).",
          "Can also mean a couple in a relationship (par)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "ett par + plural noun",
            "meaning_en": "a couple of; a small number",
            "note_en": "Often means about two, sometimes just ‘a few’."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ett par + time/distance word (år, månader, mil, meter)",
            "meaning_en": "about two (time/distance)",
            "note_en": ""
          },
          {
            "pattern_id": "pat_3",
            "pattern": "ett par + clothing/object that comes in twos",
            "meaning_en": "a pair (matching items)",
            "note_en": "Common with byxor and similar items."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "par (plural) + adjective (e.g., monogama par)",
            "meaning_en": "couples/pairs (in a relationship)",
            "note_en": ""
          }
        ],
        "common_errors_en": [
          {
            "error": "Using singular after ett par (e.g., *ett par sak).",
            "fix": "Use plural: ett par saker.",
            "explain_en": "After ett par, Swedish normally uses a plural noun."
          },
          {
            "error": "Confusing ett par with några in exact counting contexts.",
            "fix": "Use två when you mean exactly two.",
            "explain_en": "Ett par often means ‘about two’ or ‘a small number’, not always exactly two."
          }
