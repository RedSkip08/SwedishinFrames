      {
        "sense_id": "lu_lernen:1",
        "frame_id": "frame_education",
        "meaning_en": "get knowledge or a skill through study, practice, or experience.",
        "how_to_use_en": [
          "Often used as reflexive: lära sig + language/skill/fact.",
          "Common with an infinitive for skills: lära sig + verb (e.g., flyga).",
          "Can be used with om for facts: lära sig om + topic."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "lära sig + språk/ämne",
            "meaning_en": "learn a language/subject",
            "note_en": "Typically used for languages (e.g., franska, arabiska, engelska)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "lära sig + infinitiv",
            "meaning_en": "learn to do something",
            "note_en": "Used for skills/actions (e.g., lära sig flyga)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "lära sig hur + bisats",
            "meaning_en": "learn how to do something",
            "note_en": "Introduces a method or procedure."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "lära sig om + ämne",
            "meaning_en": "learn about a topic",
            "note_en": "Used for facts/information (e.g., om äpplets historia)."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "det tog (tid) för + person + att lära sig + X",
            "meaning_en": "it took time to learn",
            "note_en": "Common when talking about how long learning took."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *lära* without sig for this meaning (e.g., *Jag lär svenska*).",
            "fix": "Use reflexive: Jag lär mig svenska.",
            "explain_en": "For ‘learn (acquire)’, Swedish normally uses lära sig."
          },
          {
            "error": "Mixing up lära sig (learn) and lära ut (teach).",
            "fix": "Use lära sig for the learner; lära ut for the teacher.",
            "explain_en": "They describe opposite roles in education."
          }
        ],
