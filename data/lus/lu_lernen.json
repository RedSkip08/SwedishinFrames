    "senses": [
      {
        "sense_id": "lu_lernen:1",
        "frame_id": "frame_learning_&_studying",
        "meaning_en": "get knowledge or a skill through practice, study, or experience",
        "how_to_use_en": [
          "Use with a reflexive pronoun: jag lär mig, du lär dig, hen lär sig.",
          "Often followed by what you learn: a language, a skill, or about a topic.",
          "Common with purpose: för att lära sig + infinitive."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "lära sig + språk/ämne",
            "meaning_en": "learn a language/subject",
            "note_en": "Often used with languages: lära sig franska/italienska."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "lära sig + (att) + infinitive",
            "meaning_en": "learn to do something",
            "note_en": "Att is optional: lära sig flyga; lära sig att programmera."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "lära sig om + topic",
            "meaning_en": "learn about a topic",
            "note_en": "Used for information rather than a practical skill."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "det tog + tid + att lära sig + X",
            "meaning_en": "it took time to learn",
            "note_en": "Expresses duration: det tog ungefär en månad att lära sig ett språk."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "för att lära sig + infinitive",
            "meaning_en": "in order to learn",
            "note_en": "Often with a place/institution: till skola/klasser för att lära sig läsa."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *lära* without reflexive pronoun for this meaning.",
            "fix": "Use lära sig when the subject is the learner.",
            "explain_en": "Lära (without sig) often means 'teach' someone else, not 'learn'."
          },
          {
            "error": "Wrong reflexive pronoun: *han lär mig* (when he is learning).",
            "fix": "Use han lär sig.",
            "explain_en": "The reflexive pronoun must match the subject (mig/dig/sig/oss/er/sig)."
          }
        ],
