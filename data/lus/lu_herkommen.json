        "sense_id": "lu_herkommen:1",
        "frame_id": "frame_origin",
        "meaning_en": "be from a place, time, or source; have an origin.",
        "how_to_use_en": [
          "Use it to say where something or someone originates.",
          "Follow with a place, time period, direction, or source.",
          "Often used with modal verbs like kan, måste, skulle, or with sägs/tros/antas."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "[Entity] kan komma från [Origin]",
            "meaning_en": "may originate from",
            "note_en": "Used for possibility (e.g., winds, threats, radiation)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "[Entity] sägs/tros/antas komma från [Origin]",
            "meaning_en": "is said/believed to come from",
            "note_en": "Used when the origin is uncertain or reported."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "[Entity] skulle till en början komma från [Origin]",
            "meaning_en": "would initially come from",
            "note_en": "Adds a time phrase to show when the source applies."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "[Entity] måste komma från [Origin]",
            "meaning_en": "must come from",
            "note_en": "Used for strong conclusion about the source."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "[Entity] komma från [Origin] ... upp genom [Path]",
            "meaning_en": "come from ... via a route",
            "note_en": "Adds a path/route after stating the origin."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *komma från* without stating the origin.",
            "fix": "Add an origin after från.",
            "explain_en": "From is required: you normally say what the origin is (place, time, source, etc.)."
          },
          {
            "error": "Confusing *komma från* (origin) with *komma till* (destination).",
            "fix": "Use *komma från* for where it starts; *komma till* for where it goes.",
            "explain_en": "The preposition changes the meaning: from = origin, to = destination."
          }
        ],
