        "sense_id": "lu_romanze_die:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "a romantic relationship, often new, secret, or not very serious",
        "how_to_use_en": [
          "Use for a romance between two people (often in stories).",
          "Common with verbs like inleda, ha, växa, bli avbruten.",
          "Often followed by mellan X och Y or med someone."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "en romans mellan [Partner_1] och [Partner_2]",
            "meaning_en": "a romance between two people",
            "note_en": "Very common when both partners are named."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "[Partner_1] inleder en romans",
            "meaning_en": "start a romance",
            "note_en": ""
          },
          {
            "pattern_id": "pat_3",
            "pattern": "[Partner_1] har en (kort/efemär) romans med [Partner_2]",
            "meaning_en": "have a (short) romance",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "det börjar växa en romans mellan [Partner_1] och [Partner_2]",
            "meaning_en": "a romance begins to grow",
            "note_en": "Often used for something that develops slowly."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "[Partners] romans blir avbruten / återuppväcka [Partners] romans",
            "meaning_en": "their romance is interrupted/returns",
            "note_en": "Uses a possessive like deras."
          }
        ],
