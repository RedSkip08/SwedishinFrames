      {
        "sense_id": "lu_kommilitone_der_kommilitone_die_kommilitonin:1",
        "frame_id": "frame_education",
        "meaning_en": "a person who studies in the same course as someone else",
        "how_to_use_en": [
          "Use about someone in the same course/programme, often in school or university contexts.",
          "Common with the preposition med when saying two people were course mates."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X var kurskamrat med Y",
            "meaning_en": "X and Y studied the same course",
            "note_en": "Often used when describing past studies."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X blev kurskamrat med Y",
            "meaning_en": "X became a course mate with Y",
            "note_en": "Used when someone enrolls/starts at an institution."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "en kurskamrat från + institution",
            "meaning_en": "a course mate from an institution",
            "note_en": "Adds where the shared studies took place."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using it for any friend at school (not the same course).",
            "fix": "Use klasskamrat for same class, or vän for friend.",
            "explain_en": "kurskamrat focuses on sharing a specific course of study."
          },
          {
            "error": "Forgetting med after kurskamrat in this construction.",
            "fix": "Say: vara/blir kurskamrat med + person.",
            "explain_en": "In the examples, the relation is expressed with med."
          }
