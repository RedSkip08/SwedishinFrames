    "senses": [
      {
        "sense_id": "lu_kommilitone_der_kommilitone_die_kommilitonin:1",
        "frame_id": "frame_people",
        "meaning_en": "a person who takes the same course as someone else",
        "how_to_use_en": [
          "Use about someone you study the same course/program with.",
          "Often used with “vara/blir … med” to name the other person."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X var kurskamrat med Y",
            "meaning_en": "X and Y took the same course",
            "note_en": "Common in biographies to link two people."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X blev kurskamrat med Y",
            "meaning_en": "X became a course mate of Y",
            "note_en": "Often when someone enrolls or starts at a school."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X blev kurskamrat med Y på/vid [institution]",
            "meaning_en": "became course mates at an institution",
            "note_en": "The institution can be a school or university."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "en kurskamrat från [institution]",
            "meaning_en": "a course mate from an institution",
            "note_en": "Used when the shared course is linked to a specific place."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using kurskamrat for any friend at school.",
            "fix": "Use klasskamrat or skolkamrat if it is about the same class/school.",
            "explain_en": "Kurskamrat focuses on sharing a specific course (often at college/university)."
          },
          {
            "error": "Saying *min kurskamrat med Anna*.",
            "fix": "Say “Jag är kurskamrat med Anna” or “Anna är min kurskamrat”.",
            "explain_en": "“med” is used with the verb phrase “vara/blir kurskamrat med …”, not after “min”."
          }
        ],
