    "senses": [
      {
        "sense_id": "lu_student_der_student__die_studentin:1",
        "frame_id": "frame_people",
        "meaning_en": "a person who studies, especially at a university or similar school",
        "how_to_use_en": [
          "Use for someone enrolled in education (often university).",
          "Often followed by a place/institution: i Uppsala, vid ett universitet."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är student",
            "meaning_en": "X is a student",
            "note_en": "States someone’s role/occupation."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "bli student (vid + Institution)",
            "meaning_en": "become a student at",
            "note_en": "Common in biographies for starting studies at an institution."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "student vid + Institution",
            "meaning_en": "student at an institution",
            "note_en": "Used to specify where someone studies."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "student i + Place",
            "meaning_en": "student in a place",
            "note_en": "Used when the city stands for the university there."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "som student (på/vid + Institution)",
            "meaning_en": "as a student at",
            "note_en": "Gives background for what someone did while studying."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *student* for a school pupil in general.",
            "fix": "Use elev for a pupil; use student mainly for higher education.",
            "explain_en": "In Swedish, student often means university/college student, not any learner."
          }
        ],
