      {
        "sense_id": "lu_geschieden:1",
        "frame_id": "frame_marriage",
        "meaning_en": "not married anymore because the marriage has ended legally.",
        "how_to_use_en": [
          "Use after vara/bliva to state marital status: är skild.",
          "You can add time words like nu/numera.",
          "You can mention the ex-partner with från/ifrån."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är skild",
            "note_en": "Basic statement of being divorced."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X är nu/numera skild",
            "note_en": "Adds a time perspective (“now/nowadays”)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X var skild från/ifrån Y",
            "note_en": "Mentions the former spouse/partner."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X har varit gift och skild (två/tre) gånger",
            "note_en": "Talks about repeated marriages/divorces."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "om man är ogift, änkling eller skild",
            "note_en": "Listed as an official marital status category."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *skild* to mean “separated” in general (not about marriage).",
            "fix": "Use *separerad* for living apart; use *skild* for legally divorced.",
            "explain_en": "In these examples, *skild* is a civil status (divorced), often listed with *ogift* and *änkling*."
          }
        ],
