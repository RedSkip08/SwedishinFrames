        "sense_id": "lu_kita_die:1",
        "frame_id": "frame_education",
        "meaning_en": "an institution where young children go before starting school",
        "how_to_use_en": [
          "Use it for the place/institution (not the activity).",
          "Often mentioned together with skola/grundskola when describing a town."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "Här/I [plats] finns (bland annat) (en) förskola",
            "meaning_en": "there is a preschool in a place",
            "note_en": "Common in factual descriptions of towns/areas."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "En förskola finns, där [barn] går",
            "meaning_en": "children attend that preschool",
            "note_en": "Uses gå to mean “attend” (go to)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "(en) förskola och (en) skola/grundskola",
            "meaning_en": "a preschool and a school",
            "note_en": "Often listed as local institutions."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "arbeta som [yrke] vid en förskola",
            "meaning_en": "work at a preschool",
            "note_en": "Vid = “at” (workplace)."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "göras om till förskola",
            "meaning_en": "be converted into a preschool",
            "note_en": "Used when a building changes function."
          }
        ],
