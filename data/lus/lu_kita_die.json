    "senses": [
      {
        "sense_id": "lu_kita_die:1",
        "frame_id": "frame_education",
        "meaning_en": "a place where young children go before primary school",
        "how_to_use_en": [
          "Use it for the institution (a preschool), often mentioned with other local services.",
          "Common with existential phrases like “det finns …” and location phrases like “i X finns …”.",
          "Often used with “en” in singular: “en förskola”."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "Här finns (bland annat) ... förskola ...",
            "meaning_en": "there is a preschool (here)",
            "note_en": "Used to list what a place has."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "På orten/I [plats] finns en förskola",
            "meaning_en": "there is a preschool in a place",
            "note_en": "Common in factual descriptions of towns/areas."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "det finns ... skola, förskola ...",
            "meaning_en": "a place has school and preschool",
            "note_en": "Often appears in lists of services."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "göra om ... till förskola",
            "meaning_en": "change something into a preschool",
            "note_en": "Uses “till” to show the new function."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "arbeta som ... vid en förskola",
            "meaning_en": "work at a preschool",
            "note_en": "“vid” is a common preposition for workplaces."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *förskola* for primary school.",
            "fix": "Use “grundskola” for primary/compulsory school; use “förskola” for preschool.",
            "explain_en": "In the examples, “förskola” is listed separately from “skola/grundskola”."
          },
          {
            "error": "Wrong article: *ett förskola*.",
            "fix": "Say “en förskola”.",
            "explain_en": "The noun is en-gender: en förskola, förskolan."
          }
        ],
