      {
        "sense_id": "lu_frau_die:1",
        "frame_id": "frame_marriage",
        "meaning_en": "a woman who is married to a man; his wife.",
        "how_to_use_en": [
          "Often used with a possessive: hans fru, sin fru.",
          "Common in factual/biographical style, often followed by her name."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X och hans fru Y",
            "note_en": "Lists a man and his wife, often with her name."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X och sin fru Y",
            "note_en": "Refers back to the subject (X) and his wife."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "gifta sig med sin första fru (Y) + Time",
            "note_en": "Marriage event with “first wife”, often with a year."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "bo (tillsammans) med (sin) fru + Place",
            "note_en": "Living with one’s wife, optionally with a place."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "Y är sedan Time hans fru",
            "note_en": "States marital status since a time point."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using fru for any adult woman (like English “woman/lady”).",
            "fix": "Use kvinna/dam for “woman/lady”; use fru for “wife” (or as a title).",
            "explain_en": "In these examples, fru refers to the spouse in a marriage, not just a woman."
          }
