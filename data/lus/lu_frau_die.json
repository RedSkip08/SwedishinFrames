    "senses": [
      {
        "sense_id": "lu_frau_die:1",
        "frame_id": "frame_marriage",
        "meaning_en": "a married woman in relation to her spouse; a wife",
        "how_to_use_en": [
          "Use for a married woman, often in formal or written style.",
          "Common with possessives: sin/hans/hennes + hustru, or with a name."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "sin/hans/hennes + hustru (+ Namn)",
            "meaning_en": "someone’s wife",
            "note_en": "Very common in biographies and news-style writing."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X:s hustru, Namn",
            "meaning_en": "X’s wife (named)",
            "note_en": "Genitive with -s is common with names: Dieulafoys hustru."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "tredje + hustru (+ Namn)",
            "meaning_en": "third wife",
            "note_en": "Ordinal numbers can specify which marriage."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using hustru for any female partner (not married).",
            "fix": "Use partner/sambo/flickvän depending on the relationship.",
            "explain_en": "Hustru normally implies marriage."
          },
          {
            "error": "Mixing up sin and hans/hennes before hustru.",
            "fix": "Use sin hustru when the subject owns the relation; otherwise hans/hennes hustru.",
            "explain_en": "Sin refers back to the subject; hans/hennes refers to someone else."
          }
