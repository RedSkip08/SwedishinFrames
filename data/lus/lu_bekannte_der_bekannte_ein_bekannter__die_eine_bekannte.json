      {
        "sense_id": "lu_bekannte_der_bekannte_ein_bekannter__die_eine_bekannte:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "a person you know a little, but who is not a close friend",
        "how_to_use_en": [
          "Use en bekant to talk about someone you know (often not very well).",
          "Often used with till: en bekant till X.",
          "For the relationship, use bekantskap."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "en (gammal) bekant",
            "meaning_en": "an (old) acquaintance",
            "note_en": "Often used when you meet someone you already know."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "en bekant till + person/NP",
            "meaning_en": "an acquaintance of",
            "note_en": "Shows whose acquaintance the person is."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "vara bekant med + person/NP",
            "meaning_en": "be acquainted with someone",
            "note_en": "Describes an existing relationship of knowing someone."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "bli bekant med + person/NP",
            "meaning_en": "get acquainted with someone",
            "note_en": "Focuses on the start of knowing someone."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using bekant as a noun when you mean “familiar” (about a place/thing).",
            "fix": "Use bekant as an adjective for places/things, or use en bekant only for a person.",
            "explain_en": "In the examples, bekant can be a noun (a person) or an adjective (familiar)."
          },
          {
            "error": "Saying *en bekant med någon* for “an acquaintance with someone”.",
