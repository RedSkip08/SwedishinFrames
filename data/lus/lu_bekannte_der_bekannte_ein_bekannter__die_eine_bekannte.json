        "meaning_en": "a person you know, but not a close friend",
        "how_to_use_en": [
          "Use about someone you have met and know a little.",
          "Often used with a possessor: en bekant till X / familjens bekant.",
          "Add an adjective to show closeness or time: nära, gammal."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "träffa [Person], som är en (gammal) bekant",
            "meaning_en": "meet someone who is an acquaintance",
            "note_en": "Often used when you meet someone you already know."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "en bekant till [Person]",
            "meaning_en": "an acquaintance of someone",
            "note_en": "Shows the connection via another person."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "[Någon] var en bekant från [Plats]",
            "meaning_en": "an acquaintance from a place",
            "note_en": "Adds where you know the person from."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "[Någon] var nära bekant med [Någon]",
            "meaning_en": "be closely acquainted with",
            "note_en": "This is a stronger relationship than just bekant."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "familjens bekant",
            "meaning_en": "the family’s acquaintance",
            "note_en": "Possessive form for a person known by a group."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using bekant when you mean a close friend.",
            "fix": "Use vän (or nära vän) for a real friend.",
