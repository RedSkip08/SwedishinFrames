          },
          {
            "pattern_id": "pat_2",
            "pattern": "utläsa/höra … varifrån [Entity] + verb",
            "meaning_en": "tell where someone/something comes from",
            "note_en": "Used with evidence like numbers or dialect."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "teorier om varifrån [Entity] kommer",
            "meaning_en": "theories about an origin",
            "note_en": "Common with words, ideas, traditions, etc."
