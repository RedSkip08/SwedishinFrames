    "senses": [
      {
        "sense_id": "lu_bibliothek_die:1",
        "frame_id": "frame_education",
        "meaning_en": "a place or institution where people can read or borrow books and other materials",
        "how_to_use_en": [
          "Use it for a public, school, or museum library as an institution or place.",
          "Often used with a possessor: a town’s, school’s, or museum’s library.",
          "Can be described with adjectives like offentligt or vetenskapligt."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X:s bibliotek",
            "meaning_en": "someone’s/something’s library",
            "note_en": "Common for institutions and places (e.g., a museum’s or a village’s library)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ett bibliotek i + plats",
            "meaning_en": "a library in a place",
            "note_en": ""
          },
          {
            "pattern_id": "pat_3",
            "pattern": "i/på + (platsens) bibliotek",
            "meaning_en": "at/in the library",
            "note_en": "Used for location (e.g., at a library in a city)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "ett + adj + bibliotek",
            "meaning_en": "a library of a certain type",
            "note_en": "For example offentligt, vetenskapligt."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "ha ett bibliotek med + material",
            "meaning_en": "have a library with materials",
            "note_en": "Often followed by what it contains, e.g., many books."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using en: *en bibliotek",
            "fix": "Use ett: ett bibliotek",
            "explain_en": "Bibliotek is a neuter noun (ett-word)."
          },
          {
            "error": "Making plural with -er: *biblioteker",
            "fix": "Use bibliotek (indefinite plural) / biblioteken (definite plural)",
            "explain_en": "The indefinite plural is the same as the singular: bibliotek."
          }
        ],
