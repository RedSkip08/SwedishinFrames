    "senses": [
      {
        "sense_id": "lu_aus:1",
        "frame_id": "frame_origin",
        "meaning_en": "shows where someone or something comes from, or the starting point in time, distance, or comparison",
        "how_to_use_en": [
          "Put från before a place, time, or source.",
          "Use it for a starting point: time, distance, or origin.",
          "It can also mark what something differs from or is prevented from."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X från [plats/land]",
            "meaning_en": "origin from a place",
            "note_en": "Used for people/things with a geographical background."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "från [årtal/tid] (till/fram till Y)",
            "meaning_en": "starting time",
            "note_en": "Often paired with till or fram till for an end point."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "[antal] kilometer från [plats]",
            "meaning_en": "distance from a place",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "skilja sig från [något]",
            "meaning_en": "differ from something",
            "note_en": "Used in comparisons."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "rädda/stoppa någon från att + infinitive",
            "meaning_en": "prevent someone from doing",
            "note_en": "Från introduces the unwanted action."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using från when you mean direction/goal (to).",
            "fix": "Use till for the goal: till Stockholm.",
            "explain_en": "Från marks the source/start; till marks the destination."
          },
          {
            "error": "Forgetting att after från before a verb.",
            "fix": "Use från att + infinitive: från att begå ...",
            "explain_en": "In this use, från introduces an action that is avoided/prevented."
          }
        ],
