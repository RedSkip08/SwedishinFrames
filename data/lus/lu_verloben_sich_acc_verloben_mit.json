    "senses": [
      {
        "sense_id": "lu_verloben_sich_acc_verloben_mit:1",
        "frame_id": "frame_marriage",
        "meaning_en": "to decide and announce you will marry someone; become engaged.",
        "how_to_use_en": [
          "Use about two people who plan to marry: X och Y förlovar sig.",
          "Often add the partner with \"med\": förlova sig med någon.",
          "Can be used with modal verbs: vilja/överväga/valde att förlova sig."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "SUBJEKT + förlova sig",
            "note_en": "Often a couple or two people as subject (e.g., paret)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "SUBJEKT + förlova sig + med + PERSON",
            "note_en": "Names/pronouns can fill the partner role (e.g., med Nicola, med Norman)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "SUBJEKT + ska/ville/valde/övervägde + (att) + förlova sig",
            "note_en": "Common with future plans, wishes, choices, or considerations."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "TID + SUBJEKT + förlova sig",
            "note_en": "Time expressions can be added (e.g., under marknadsdagarna; sex månader senare)."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *förlova* without \"sig\" for this meaning.",
            "fix": "Use the reflexive form: \"förlova sig\".",
            "explain_en": "In these examples the engagement is expressed with the reflexive verb (someone gets engaged)."
          },
          {
            "error": "Saying *förlova sig till* någon.",
            "fix": "Say \"förlova sig med\" någon.",
            "explain_en": "The partner is introduced with \"med\" in the examples."
          }
