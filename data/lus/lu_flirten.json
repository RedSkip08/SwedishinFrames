      {
        "sense_id": "lu_flirten:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "try to show romantic or playful interest in someone",
        "how_to_use_en": [
          "Use flirta med + person for who you flirt with.",
          "Use flirta (alone) for flirting in general.",
          "You can add adverbs like tydligt to say how someone flirts."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X flirtar med Y",
            "meaning_en": "X flirts with Y",
            "note_en": "Most common pattern for naming the other person."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X börjar (att) flirta med Y",
            "meaning_en": "X starts flirting with Y",
            "note_en": ""
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X försöker flirta med Y",
            "meaning_en": "X tries to flirt with Y",
            "note_en": "Often implies it may not succeed."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X väljer att (tydligt) flirta med Y",
            "meaning_en": "X chooses to flirt with Y",
            "note_en": "Adverbs like tydligt can describe the flirting."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "X tycker om att flirta",
            "meaning_en": "X likes to flirt",
            "note_en": "Used for general habit, without naming a person."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *flirta till* someone.",
            "fix": "Use flirta med someone.",
            "explain_en": "In the examples, the person you flirt with is introduced by med."
          },
          {
            "error": "Mixing up flirta and ligga (with).",
            "fix": "Use flirta for showing interest; ligga (med) is about sex.",
            "explain_en": "Flirta is about flirting/attraction, not necessarily a sexual relationship."
          }
        ],
