    "senses": [
      {
        "sense_id": "lu_flirten:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "try to show romantic interest in someone, often playfully and not very seriously.",
        "how_to_use_en": [
          "Often used with med + person: flirta med någon.",
          "Can be used without an object: tycka om/älska att flirta.",
          "You can add tillbaka when someone responds: flirta tillbaka."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X flirta med Y",
            "note_en": "Most common pattern: one person shows romantic interest toward another."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X försöker flirta med Y",
            "note_en": "Emphasizes an attempt (maybe not successful)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X börjar att flirta med Y",
            "note_en": "Focuses on the start of flirting."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X väljer att (tydligt) flirta med Y",
            "note_en": "Highlights a deliberate choice; adverbs like tydligt can be added."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "X vägrar flirta tillbaka",
            "note_en": "Back-and-forth flirting: someone refuses to respond."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using wrong preposition: *flirta till någon.",
            "fix": "Use med: flirta med någon.",
            "explain_en": "In the examples, the person you flirt with is introduced by med."
          },
          {
            "error": "Confusing flirta with dejta.",
            "fix": "Use dejta for an actual date; use flirta for showing interest.",
            "explain_en": "Flirta is about behavior/signals; it does not mean you are in a relationship."
          }
        ],
