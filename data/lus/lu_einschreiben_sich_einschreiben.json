        "sense_id": "lu_einschreiben_sich_einschreiben:1",
        "frame_id": "frame_classes_&_courses",
        "meaning_en": "register yourself to attend a school, university, or similar institution.",
        "how_to_use_en": [
          "Use when someone officially becomes a student/member at an institution.",
          "Often followed by på/vid + institution name."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "skriva in sig på + Institution",
            "meaning_en": "enroll at a school/institution",
            "note_en": "Common with skola and similar places."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "skriva in sig vid + Institution",
            "meaning_en": "enroll at a university/institution",
            "note_en": "Often used with universitet."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "låta + någon + skriva in sig (på/vid + Institution)",
            "meaning_en": "have someone enrolled",
            "note_en": "Means arranging that the person gets enrolled."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "vägra att skriva in sig",
            "meaning_en": "refuse to enroll",
            "note_en": ""
          },
          {
            "pattern_id": "pat_5",
            "pattern": "hindras/förbjudas att skriva in sig (vid + Institution)",
            "meaning_en": "be prevented from enrolling",
            "note_en": ""
          }
        ],
        "common_errors_en": [
