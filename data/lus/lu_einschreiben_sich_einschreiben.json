    "senses": [
      {
        "sense_id": "lu_einschreiben_sich_einschreiben:1",
        "frame_id": "frame_education",
        "meaning_en": "register yourself as a student at a school, university, or similar institution.",
        "how_to_use_en": [
          "Use it when someone becomes officially registered at an institution.",
          "Often followed by på/vid + institution name."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X skriver in sig på Y",
            "meaning_en": "enroll at a school/place",
            "note_en": "Common with skola and similar places."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X skriver in sig vid Y",
            "meaning_en": "enroll at an institution",
            "note_en": "Often used with universitet and formal institutions."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X låter skriva in sig (vid/på Y)",
            "meaning_en": "have oneself enrolled",
            "note_en": "More formal/older style; someone arranges the enrollment."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X vägrar att skriva in sig",
            "meaning_en": "refuse to enroll",
            "note_en": ""
          },
          {
            "pattern_id": "pat_5",
            "pattern": "X hindras/förbjuds att skriva in sig vid Y",
            "meaning_en": "be prevented from enrolling",
            "note_en": "Passive meaning: someone else stops the enrollment."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using it without sig: *skriva in på skolan",
            "fix": "skriva in sig på skolan",
            "explain_en": "In this sense it is reflexive: the person enrolls themself."
          },
          {
            "error": "Mixing prepositions: *skriva in sig i skolan (in this meaning)",
            "fix": "skriva in sig på skolan / vid universitetet",
            "explain_en": "The examples use på (school) and vid (university/institution)."
          }
        ],
