        "meaning_en": "a topic or field that you study or teach in school or at a university",
        "how_to_use_en": [
          "Use for school/university subjects (e.g., teknik, filosofi).",
          "Often used with adjectives like teoretiskt, akademiskt, obligatoriskt.",
          "Common with prepositions: inom (within) and vid (at an institution)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är ett ämne (inom Y)",
            "meaning_en": "X is a subject (within Y)",
            "note_en": "Used to classify a field inside a broader field."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ämnet + (namn)",
            "meaning_en": "the subject + name",
            "note_en": "Common when listing or comparing school subjects."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "ett + adjektiv + ämne",
            "meaning_en": "a + adjective + subject",
            "note_en": "E.g. teoretiskt, akademiskt, obligatoriskt, tvärvetenskapligt."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "samma ämne",
            "meaning_en": "the same subject",
            "note_en": "Used when someone studies/teaches the same field again."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "ämne vid + institution",
            "meaning_en": "subject at an institution",
            "note_en": "Used for universities/högskolor where the subject exists."
          }
        ],
