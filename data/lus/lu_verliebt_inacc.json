      {
        "sense_id": "lu_verliebt_inacc:1",
        "frame_id": "frame_dating",
        "meaning_en": "having romantic love for someone; being in love.",
        "how_to_use_en": [
          "Use vara kär i + person to say you are in love with someone.",
          "Use bli kär (i + person) to talk about falling in love.",
          "You can add an adverb like olyckligt to show how it feels."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är kär i Y",
            "note_en": "State: Partner_1 is in love with Partner_2."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X blir kär (i Y)",
            "note_en": "Stage/change: Partner_1 falls in love (optionally with Partner_2)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X är (adverb) kär i Y",
            "note_en": "Adverb can describe the feeling, e.g. olyckligt."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *kär med* someone.",
            "fix": "Use kär i someone.",
            "explain_en": "In Swedish the normal preposition is i: vara kär i honom/henne."
          },
          {
            "error": "Saying *är kär* when you mean the moment it starts.",
            "fix": "Use blir kär for the change.",
            "explain_en": "är kär = the state; blir kär = the start (fall in love)."
          }
