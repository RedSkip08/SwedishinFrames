    "senses": [
      {
        "sense_id": "lu_verliebt_inacc:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "having romantic love for someone",
        "how_to_use_en": [
          "Use with vara: vara kär (i någon).",
          "Often followed by i + person (kär i henne).",
          "Can be used with bli to mean “fall in love”."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är kär i Y",
            "meaning_en": "X is in love with Y",
            "note_en": "Most common way to express being in love with someone."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X blir kär i Y",
            "meaning_en": "X falls in love with Y",
            "note_en": "Focuses on the change into being in love."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X har varit kär i Y",
            "meaning_en": "X has been in love with Y",
            "note_en": "Often used with a time expression (e.g., “so länge ...”)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X är (olyckligt) kär i Y",
            "meaning_en": "X is (unhappily) in love",
            "note_en": "An adverb like olyckligt can describe the situation."
          }
        ],
        "common_errors_en": [
          {
            "error": "Jag är kär med honom.",
            "fix": "Jag är kär i honom.",
            "explain_en": "With kär, Swedish normally uses i (kär i någon), not med."
