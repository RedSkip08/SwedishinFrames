      {
        "sense_id": "lu_unterrichten:1",
        "frame_id": "frame_education",
        "meaning_en": "teach someone a subject or skill, often in a school or course setting.",
        "how_to_use_en": [
          "Use for giving lessons or instruction to learners.",
          "Often followed by i + subject, or by a direct object (the learners).",
          "Can be used without object when the activity is clear."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "undervisa i + Ämne",
            "meaning_en": "teach a subject",
            "note_en": "Used for school/university subjects and fields."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "undervisa + Person/Grupper",
            "meaning_en": "teach someone/others",
            "note_en": "Focuses on the learners rather than the subject."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "undervisa + Person/Grupper + i + Ämne",
            "meaning_en": "teach someone a subject",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "undervisa + Person/Grupper + i + Färdighet",
            "meaning_en": "teach someone how to do something",
            "note_en": "Often followed by an embedded question/statement like hur ...."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "undervisa i + Ämne + vid + Institution",
            "meaning_en": "teach a subject at an institution",
            "note_en": "Common with universities and schools."
          }
        ],
        "common_errors_en": [
