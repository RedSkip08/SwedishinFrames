      {
        "sense_id": "lu_unterrichten:1",
        "frame_id": "frame_education",
        "meaning_en": "to teach someone a subject, skill, or information, often in a school or course setting",
        "how_to_use_en": [
          "Use for giving lessons or instruction (in a subject, skill, or facts).",
          "Often with i + subject, or with a direct object (the learners).",
          "Can be used with a place/institution (e.g., at a school or college)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "undervisa i + ämne",
            "meaning_en": "teach a subject",
            "note_en": "Common with school subjects and fields."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "undervisa + person/grupp",
            "meaning_en": "teach someone",
            "note_en": "The object is the learner(s), e.g. andra, barn, dem."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "undervisa + person/grupp + i + ämne",
            "meaning_en": "teach someone a subject",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "undervisa + person + i + färdighet",
            "meaning_en": "teach someone a skill",
            "note_en": "Used for training, e.g. dance."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "undervisa + plats/institution",
            "meaning_en": "teach at a place",
            "note_en": "Often with a school, college, or city."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *undervisa* without i when you name the subject (e.g., *undervisa matematik*).",
            "fix": "Say *undervisa i matematik*.",
            "explain_en": "In these examples, the subject is introduced with i."
          },
          {
            "error": "Mixing up who is taught: *undervisa* + teacher as object.",
            "fix": "Make the learners the object: *undervisa barn/dem/andra*.",
            "explain_en": "The direct object is typically the students/learners."
          }
        ],
