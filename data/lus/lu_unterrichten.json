    "senses": [
      {
        "sense_id": "lu_unterrichten:1",
        "frame_id": "frame_teaching",
        "meaning_en": "give lessons to help someone learn a subject or skill",
        "how_to_use_en": [
          "Use for teaching a subject, skill, or knowledge, often in a school or course.",
          "Often takes i + subject, and can also take a direct object (the learners).",
          "Can be used without an object when the context is clear."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "undervisa i + ämne",
            "meaning_en": "teach a subject",
            "note_en": "Common for school subjects and languages."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "undervisa + person/grupp",
            "meaning_en": "teach someone",
            "note_en": "The learner can be a person or group (e.g., andra, barn)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "undervisa + person/grupp + i + ämne",
            "meaning_en": "teach someone a subject",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "undervisa (vid/på) + institution",
            "meaning_en": "teach at an institution",
            "note_en": "Often with vid (e.g., vid Merton College) or at schools."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "undervisa + adverb (t.ex. tvärvetenskapligt)",
            "meaning_en": "teach in a certain way",
            "note_en": "An adverb can describe the teaching method."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *undervisa* for “study/learn” (e.g., *Jag undervisar svenska* meaning “I study Swedish”).",
            "fix": "Use *studera* or *lära sig* for learning; use *undervisa* only for teaching.",
            "explain_en": "Undervisa describes what the teacher does, not what the learner does."
          },
          {
            "error": "Mixing up *undervisa i* and *undervisa på*.",
            "fix": "Use *undervisa i* for the subject; use *undervisa på* for the language of instruction.",
            "explain_en": "In the examples, the subject is introduced with i (e.g., i tyska)."
          }
        ],
