        "sense_id": "lu_verlobung_die:1",
        "frame_id": "frame_marriage",
        "meaning_en": "an agreement/status of being promised to marry someone",
        "how_to_use_en": [
          "Use for the relationship/status before marriage: en förlovning.",
          "Often with possessives: sin/deras/parets förlovning.",
          "Common with verbs like leda till, tillkännage, fira, bryta."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "leda till en förlovning",
            "note_en": "The engagement is the result of earlier events."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "bryta/bryta sin/deras förlovning",
            "note_en": "To end the engagement."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "tillkännage/meddela (sin/deras) förlovning",
            "note_en": "To announce the engagement."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "fira sin förlovning (med X)",
            "note_en": "To celebrate the engagement, often with the partner."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "[tid] års förlovning",
            "note_en": "Duration of the engagement (e.g., åtta års förlovning)."
          }
        ],
        "common_errors_en": [
