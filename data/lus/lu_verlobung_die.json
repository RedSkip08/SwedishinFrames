        "meaning_en": "an agreement between two partners to marry in the future",
        "how_to_use_en": [
          "Use it for the engagement status or the engagement event.",
          "Often used with possessives: sin/deras förlovning.",
          "Common with verbs like leda till, tillkännage, fira, bryta."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "leda till en förlovning",
            "meaning_en": "result in an engagement",
            "note_en": ""
          },
          {
            "pattern_id": "pat_2",
            "pattern": "bryta sin/deras förlovning",
            "meaning_en": "break off an engagement",
            "note_en": "Possessive pronouns are common: sin, deras."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "efter [tid] års förlovning",
            "meaning_en": "after [time] of engagement",
            "note_en": "Used to say how long someone was engaged."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "tillkännage [någon/något] förlovning",
            "meaning_en": "announce an engagement",
            "note_en": "Often with deras/parets: deras förlovning, parets förlovning."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "fira sin förlovning (med [person])",
            "meaning_en": "celebrate an engagement",
            "note_en": "Can mention the other partner with med."
          }
        ],
        "common_errors_en": [
