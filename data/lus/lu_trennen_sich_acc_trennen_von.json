        "sense_id": "lu_trennen_sich_acc_trennen_von:1",
        "frame_id": "frame_marriage",
        "meaning_en": "to keep things or groups apart, or divide them into different parts",
        "how_to_use_en": [
          "Use it for dividing or keeping things apart (sections, groups, substances).",
          "Often used with “från” to show what is separated from what.",
          "Common in technical and formal writing."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "separera X (och) Y",
            "meaning_en": "separate two things/groups",
            "note_en": "Often about dividing into two independent parts."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "separera X från Y",
            "meaning_en": "separate X from Y",
            "note_en": "Very common when one thing is removed/kept apart from another."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "separera X från Y (av/av ... skäl)",
            "meaning_en": "separate for a reason",
            "note_en": "The reason can be stated, e.g. safety or hygiene."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "användas för att separera X",
            "meaning_en": "be used to separate",
            "note_en": "Common in descriptions of tools, signs, or methods."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "vilja separera X igen",
            "meaning_en": "want to separate again",
            "note_en": "Used when something was mixed/combined and is separated again."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *separera* without saying what is separated from what in contexts where it matters.",
            "fix": "Add “X från Y” when needed.",
            "explain_en": "Many uses specify two sides: what is separated and what it is separated from."
          },
          {
            "error": "Confusing *separera* with *separat* (adjective/adverb).",
            "fix": "Use *separera* for the action; *separat* for “separate/individually”.",
            "explain_en": "*Separera* is a verb meaning “to separate”; *separat* describes how something is."
          }
        ],
