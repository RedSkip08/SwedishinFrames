      {
        "sense_id": "lu_trennen_sich_acc_trennen_von:1",
        "frame_id": "frame_marriage",
        "meaning_en": "keep things apart by dividing them into different parts or groups.",
        "how_to_use_en": [
          "Use when you divide or keep two things/people/areas apart.",
          "Often used in formal/technical contexts (e.g., traffic, filters, machines).",
          "Common with “från” to show what is separated from what."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "separera X från Y",
            "note_en": "Separate one thing from another (e.g., grädde från mjölk; ämnen från en vätska/gas)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "separera X och Y",
            "note_en": "Separate two things/areas (e.g., bostadshus och fähus)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "separera X (igen)",
            "note_en": "Separate again/return to being separated (e.g., vätskorna igen)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "separera X i Y",
            "note_en": "Separate into parts/sections (e.g., avdelningar i en bok)."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "separera X av/av hygieniska skäl / av trafiksäkerhetsskäl",
            "note_en": "Reason phrase explaining why separation is wanted."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *separera* without saying what is separated from what when it matters.",
            "fix": "Add “X från Y” or name both parts.",
            "explain_en": "In many contexts you need to state the two sides to make the meaning clear."
          },
          {
            "error": "Confusing *separera* with relationship separation (couples).",
