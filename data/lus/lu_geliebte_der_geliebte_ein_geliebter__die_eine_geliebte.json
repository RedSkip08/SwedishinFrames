        "meaning_en": "a person someone has a romantic or sexual relationship with, often secretly",
        "how_to_use_en": [
          "Use for a romantic/sexual partner, often outside marriage or a main relationship.",
          "Often used with a possessive: hans/hennes/sin älskare."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "ha (haft) en älskare",
            "meaning_en": "have a lover",
            "note_en": "Often implies an affair."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "hans/hennes/sin älskare",
            "meaning_en": "someone’s lover",
            "note_en": "Common with possessives."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bli någons älskare",
            "meaning_en": "become someone’s lover",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "tidigare älskare",
            "meaning_en": "former lover",
            "note_en": ""
          },
          {
            "pattern_id": "pat_5",
            "pattern": "vara ... även älskare",
            "meaning_en": "also be lovers",
            "note_en": "Used when two people are lovers in addition to another relationship."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using älskare for a normal boyfriend/girlfriend in neutral contexts.",
            "fix": "Use pojkvän/flickvän or partner instead.",
            "explain_en": "Älskare often suggests an affair or a more secret relationship."
          },
          {
            "error": "Mixing up sin/hans/hennes before älskare.",
            "fix": "Use sin for the subject’s own lover; hans/hennes for someone else’s.",
            "explain_en": "Swedish reflexive possessives depend on who owns the relationship."
          }
