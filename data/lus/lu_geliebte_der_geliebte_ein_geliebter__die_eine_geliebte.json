        "meaning_en": "a person someone has a romantic or sexual relationship with, often secretly",
        "how_to_use_en": [
          "Use about a romantic/sexual partner, often outside marriage or a main relationship.",
          "Often used with a possessive: hans/hennes/sin älskare.",
          "Can be singular or plural: en älskare, två älskare."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "ha (haft) en älskare",
            "note_en": "To have/have had a lover."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "hans/hennes/sin älskare",
            "note_en": "Possessive + älskare is very common."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bli någons älskare",
            "note_en": "To become someone’s lover."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "vänta på sin älskare + (namn)",
            "note_en": "Often followed by a name."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "två älskare",
            "note_en": "Plural meaning: two people who are lovers."
          }
        ],
        "common_errors_en": [
