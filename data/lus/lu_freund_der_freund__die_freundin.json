    "senses": [
      {
        "sense_id": "lu_freund_der_freund__die_freundin:1",
        "frame_id": "frame_friendship",
        "meaning_en": "a person you have a friendship relationship with",
        "how_to_use_en": [
          "Use as a noun for someone you know well and like: hans vän, sin vän.",
          "Often with superlatives: bästa vän, bäste vän.",
          "Use with the construction vara vän med / bli vän med + person."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X:s vän (Name)",
            "note_en": "Possessive + vän to identify someone as a friend: Hans vän Max Brod; hennes vän Evanna Lynch."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "sin vän",
            "note_en": "Reflexive possessive when the subject owns the relation: bjuder han ut sin vän."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bästa vän / bäste vän",
            "note_en": "Superlative + vän for 'best friend': Min bästa vän; hans pappas bäste vän."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "vara vän med + person",
            "note_en": "To state an existing friendship: Adam är vän med Teela."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "bli vän med + person",
            "note_en": "To describe becoming friends (often with time/place): Under sin tid i Tyskland blev han vän med Carl Siegel."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *vän med* alone as a noun phrase (e.g. *min vän med Anna*).",
            "fix": "Say min vän Anna, or use the verb phrase: jag är vän med Anna.",
            "explain_en": "vän is a noun; vän med is part of the construction vara/bli vän med."
          },
          {
            "error": "Mixing up bäste and bästa.",
            "fix": "Use bäste with a male person (hans pappas bäste vän) and bästa more generally (min bästa vän).",
            "explain_en": "Both occur; bäste is a masculine form used with male referents in some styles."
          }
        ],
