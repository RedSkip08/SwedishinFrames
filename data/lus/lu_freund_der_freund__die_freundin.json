        "frame_id": "frame_friendship",
        "meaning_en": "a person you have a friendship with",
        "how_to_use_en": [
          "Use vän about a person you are friends with.",
          "Often used with a possessive: min/sin/hans/hennes vän.",
          "Can be strengthened with degree words like bäste."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X:s vän (Namn)",
            "meaning_en": "someone is X’s friend",
            "note_en": "Often with a name after vän."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "hans/hennes/sin vän",
            "meaning_en": "his/her/their friend",
            "note_en": "sin refers back to the subject."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bli vän med + person",
            "meaning_en": "become friends with someone",
            "note_en": "Describes the start of a friendship."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "bäste vän",
            "meaning_en": "best friend",
            "note_en": "A degree word makes the friendship stronger."
          }
        ],
