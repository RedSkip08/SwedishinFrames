    "senses": [
      {
        "sense_id": "lu_abitur_das_das_abitur_machen:1",
        "frame_id": "frame_education",
        "meaning_en": "a qualification showing you finished upper secondary school (gymnasium).",
        "how_to_use_en": [
          "Use it for the diploma/qualification, often with verbs like ta (“get”).",
          "Common in formal contexts about admission requirements and education history.",
          "Often used with efter (“after”) to talk about what happened next."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "leda inte till (en) gymnasieexamen",
            "meaning_en": "not result in a diploma",
            "note_en": "Used to say an education does not give this qualification."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X tog (ÅR) gymnasieexamen",
            "meaning_en": "X got a diploma",
            "note_en": "Often followed by a year."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "Efter (sin) gymnasieexamen, ...",
            "meaning_en": "after finishing upper secondary",
            "note_en": "Introduces what happened next (studies, work, travel)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "(en) gymnasieexamen vid [institution]",
            "meaning_en": "a diploma at an institution",
            "note_en": "Mentions where the qualification was obtained."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "(en) gymnasieexamen i [ämne]",
            "meaning_en": "a diploma in a subject",
            "note_en": "Adds a subject area (e.g., statsvetenskap)."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using it for the school itself (\"*på gymnasieexamen\").",
            "fix": "Use gymnasiet/gymnasieskola for the school; gymnasieexamen is the diploma.",
            "explain_en": "Gymnasieexamen names the qualification, not the institution."
          },
          {
            "error": "Mixing up with examen at university level.",
            "fix": "Use kandidatexamen/juris kandidat-examen etc. for higher education degrees.",
            "explain_en": "Gymnasieexamen is specifically upper secondary level."
          }
        ],
