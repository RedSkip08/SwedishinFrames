        "frame_id": "frame_education",
        "meaning_en": "a formal qualification showing you have finished upper secondary school (gymnasium).",
        "how_to_use_en": [
          "Use it for the qualification you get after completing gymnasium.",
          "Often used with verbs like ta (“get”) or with efter (“after”).",
          "Can be described as fullständig (“complete”)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "leda till en gymnasieexamen",
            "meaning_en": "lead to a diploma",
            "note_en": "Used about an education/program."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ta gymnasieexamen",
            "meaning_en": "get a diploma",
            "note_en": "Common in biographies and timelines."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "efter (sin) gymnasieexamen",
            "meaning_en": "after finishing upper secondary",
            "note_en": "Often followed by what someone did next (e.g., started studying)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "gymnasieexamen vid [institution]",
            "meaning_en": "diploma at an institution",
