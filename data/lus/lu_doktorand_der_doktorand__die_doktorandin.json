    "senses": [
      {
        "sense_id": "lu_doktorand_der_doktorand__die_doktorandin:1",
        "frame_id": "frame_education",
        "meaning_en": "a person studying for a doctoral degree (PhD), usually at a university or institute",
        "how_to_use_en": [
          "Use for someone doing doctoral studies at a university/institute.",
          "Often used with som (“as”) or with i + subject (“in [field]”).",
          "Can be used with an institution: vid + university/institute (“at”)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "som doktorand",
            "meaning_en": "as a PhD student",
            "note_en": "Used to describe someone’s role/status."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "doktorand vid + Institution",
            "meaning_en": "PhD student at an institution",
            "note_en": "Common with universitetet/institutet."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "doktorand i + Subject",
            "meaning_en": "PhD student in a subject",
            "note_en": "Names the research/degree field."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "anställd som doktorand",
            "meaning_en": "employed as a PhD student",
            "note_en": "In Sweden, doctoral students can be employed."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "antogs som doktorand",
            "meaning_en": "admitted as a PhD student",
            "note_en": "Used when someone is accepted into doctoral studies."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using doktorand for someone who already has a PhD.",
            "fix": "Use doktor (or forskare, depending on context).",
            "explain_en": "Doktorand is still studying; after the defence they are no longer a doktorand."
