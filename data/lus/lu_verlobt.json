        "meaning_en": "having agreed to marry someone; engaged.",
        "how_to_use_en": [
          "Use about a person’s status: X är förlovad.",
          "Add the partner with med: X är förlovad med Y.",
          "Add time with sedan + year/date: X är sedan 2011 förlovad med Y."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är förlovad",
            "note_en": "States that someone is engaged (partner not mentioned)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X är förlovad med Y",
            "note_en": "Mentions the fiancé/fiancée."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X är sedan TIME förlovad med Y",
            "note_en": "Gives the starting time of the engagement."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "TIME blev X förlovad med Y",
            "note_en": "Focuses on the event of becoming engaged."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "X var förlovad med Y",
            "note_en": "Past engagement (often followed by what happened later)."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *förlovad* for “married”.",
            "fix": "Use gift for “married”.",
            "explain_en": "förlovad means engaged (before marriage), not married."
          },
          {
            "error": "Forgetting agreement with gender/number (e.g., *ett barn är förlovad*).",
            "fix": "Use förlovat (neuter) and förlovade (plural).",
            "explain_en": "Adjectives agree: en-person: förlovad, ett: förlovat, plural: förlovade."
          }
