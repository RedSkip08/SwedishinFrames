    "senses": [
      {
        "sense_id": "lu_verlobt:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "having agreed to marry someone; engaged",
        "how_to_use_en": [
          "Use with vara: X är förlovad.",
          "Add the partner with med: förlovad med någon.",
          "Often with redan (“already”) in stories and biographies."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är förlovad",
            "meaning_en": "X is engaged",
            "note_en": "Often used without naming the partner."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X är förlovad med Y",
            "meaning_en": "X is engaged to Y",
            "note_en": "Names the partner (Y)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X har blivit förlovad med Y",
            "meaning_en": "X has become engaged to Y",
            "note_en": "Focuses on the change into being engaged."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *förlovad till* for the partner.",
            "fix": "Use förlovad med + person.",
            "explain_en": "In the examples, the partner is introduced with med."
          },
          {
            "error": "Forgetting adjective agreement: *hon är förlovad* but *det är förlovad*.",
            "fix": "Use neuter: det är förlovat; plural: de är förlovade.",
            "explain_en": "Adjectives agree with grammatical gender/number."
          }
        ],
