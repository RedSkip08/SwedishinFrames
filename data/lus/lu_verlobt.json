        "meaning_en": "having agreed to marry someone; engaged.",
        "how_to_use_en": [
          "Use with vara: X är förlovad.",
          "Often add the partner with med: förlovad med någon."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är förlovad",
            "meaning_en": "X is engaged",
            "note_en": "Often used with redan to mean “already engaged”."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X är förlovad med Y",
            "meaning_en": "X is engaged to Y",
            "note_en": "Med introduces the partner."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *förlovad* for “married”.",
            "fix": "Use gift for “married”.",
            "explain_en": "Förlovad means engaged (before marriage), not married."
          },
          {
