        "sense_id": "lu_scheidung_die:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "the legal end of a marriage between two partners",
        "how_to_use_en": [
          "Use it for the event/process of ending a marriage.",
          "Often used with prepositions like efter (“after”) and innan (“before”).",
          "Common with verbs like ansöka om, begära, ta ut, genomgå."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "innan [possessive] skilsmässa",
            "meaning_en": "before the divorce",
            "note_en": "Used to place an event earlier than the divorce."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "efter skilsmässa",
            "meaning_en": "after the divorce",
            "note_en": "Often used when describing what happened later in someone’s life."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "efter sin första skilsmässa",
            "meaning_en": "after their first divorce",
            "note_en": "Common when someone has divorced more than once."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "genomgå en (officiell) skilsmässa",
            "meaning_en": "go through a divorce",
            "note_en": "Focuses on the formal/legal process."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "ta ut skilsmässa",
            "meaning_en": "get a divorce",
            "note_en": "A common fixed expression in Swedish."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *skilsmässa* for any breakup (e.g., boyfriend/girlfriend).",
            "fix": "Use *skilsmässa* mainly for marriage; otherwise often *separation* or *göra slut*.",
            "explain_en": "In these examples, skilsmässa is about spouses/married partners."
