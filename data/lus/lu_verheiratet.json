      {
        "sense_id": "lu_verheiratet:1",
        "frame_id": "frame_marriage",
        "meaning_en": "having a husband or wife; in a marriage.",
        "how_to_use_en": [
          "Use with vara: X är/var gift.",
          "Often add the spouse with med: gift med + person.",
          "Time phrases can be added: fram till / till sin död + year."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är/var gift",
            "note_en": "States marital status without naming the spouse."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X är/var gift med Y",
            "note_en": "Names the spouse (Spouse_2) with med."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X var ... gift med Y fram till ...",
            "note_en": "Adds an end point in time (e.g., death)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X var till sin död [år] gift med Y",
            "note_en": "Time phrase before gift; common in biographies."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "Genom att X var gift ...",
            "note_en": "Reason clause: being married is given as an explanation."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *gift till* for the spouse.",
            "fix": "Use gift med + person.",
            "explain_en": "In the examples, the spouse is introduced with med (gift med Heinz Liljedahl)."
          },
          {
            "error": "Confusing gift (married) with gift (poison).",
            "fix": "Check context: marriage uses vara gift (often with med).",
            "explain_en": "Swedish gift has two common meanings; here it is marital status."
          }
        ],
