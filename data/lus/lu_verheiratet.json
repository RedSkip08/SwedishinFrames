      {
        "sense_id": "lu_verheiratet:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "legally married to someone; having a spouse.",
        "how_to_use_en": [
          "Use with vara: X är/var gift.",
          "Add med + person to say who the spouse is.",
          "Often used in biographies and facts about someone."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är/var gift",
            "meaning_en": "X is/was married",
            "note_en": "Often used without naming the spouse."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X är/var gift med Y",
            "meaning_en": "X is/was married to Y",
            "note_en": "Y is the spouse/partner in the marriage."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *gift* to mean “a present/gift”.",
            "fix": "Use present for “a gift”; gift means “married” (or “poison” in another sense).",
            "explain_en": "Swedish gift has different meanings; in these examples it means “married”."
          },
          {
            "error": "Saying *gift till* someone.",
            "fix": "Say gift med someone.",
            "explain_en": "The spouse is introduced with med in Swedish."
          }
        ],
