      {
        "sense_id": "lu_verheiratet:1",
        "frame_id": "frame_marriage",
        "meaning_en": "legally married; having a spouse.",
        "how_to_use_en": [
          "Use with vara: är/var gift.",
          "Often add med + spouse to say who someone is married to.",
          "You can add a time span to say when the marriage held."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är/var gift",
            "meaning_en": "X is/was married",
            "note_en": "States marital status without naming the spouse."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X är/var gift med Y",
            "meaning_en": "X is/was married to Y",
            "note_en": "med introduces the spouse."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X var [tidsuttryck] gift med Y",
            "meaning_en": "X was married to Y (during time)",
            "note_en": "Time can be a span (e.g., years) or “till sin död …”."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *gift* to mean “a present/gift”.",
            "fix": "Use present for “a gift”; use gift only for “married” here.",
            "explain_en": "Swedish gift can mean “married” (adjective) but “gift” in English is present."
          },
          {
            "error": "Saying *gift till* someone.",
            "fix": "Say gift med someone.",
            "explain_en": "The examples show the fixed preposition med after gift."
          }
        ],
