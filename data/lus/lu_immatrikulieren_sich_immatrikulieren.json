        "frame_id": "frame_education",
        "meaning_en": "register yourself as a student at a school, university, or similar institution.",
        "how_to_use_en": [
          "Use with reflexive sig: someone skriver in sig.",
          "Often add the place with på or vid + institution."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X skriver in sig",
            "meaning_en": "X enrolls",
            "note_en": "Focuses on the act of registering; the institution may be omitted."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X skriver in sig på + Institution",
            "meaning_en": "enroll at a school/place",
            "note_en": "Used with skolan, vårdhemmet."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X skriver in sig vid + Institution",
            "meaning_en": "enroll at a university/faculty",
            "note_en": "Common with universitet and fakultet."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X lät skriva in sig vid/på + Institution",
            "meaning_en": "had themself enrolled",
            "note_en": "Causative construction: someone arranged the enrollment."
          }
        ],
        "common_errors_en": [
