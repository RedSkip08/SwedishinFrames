          {
            "pattern_id": "pat_2",
            "pattern": "skriva in sig vid + Institution",
            "meaning_en": "enroll at an institution",
            "note_en": "Often used with universities and formal institutions."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "låta + skriva in sig + (på/vid) + Institution",
            "meaning_en": "have yourself enrolled",
            "note_en": "More formal; someone arranges the enrollment."
          },
