        "frame_id": "frame_education",
        "meaning_en": "a place for higher education and research, where people study and get degrees",
        "how_to_use_en": [
          "Use for the institution where someone studies or works.",
          "Often used with a name: e.g. Uppsala universitet.",
          "Common with prepositions like vid (“at”)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "studera vid [Institution]",
            "meaning_en": "study at a university",
            "note_en": "Very common with named universities."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "anställd som [yrke] vid [Institution]",
            "meaning_en": "work at a university",
            "note_en": "Used for jobs connected to the university."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "professor i [Subject] vid [Institution]",
            "meaning_en": "professor in a subject at a university",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "en institution vid [Institution]",
            "meaning_en": "a department at a university",
            "note_en": ""
          },
          {
            "pattern_id": "pat_5",
            "pattern": "från universitet till universitet",
            "meaning_en": "from one university to another",
            "note_en": "Fixed repetition pattern for moving between institutions."
          }
        ],
        "common_errors_en": [
