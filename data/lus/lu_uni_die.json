        "sense_id": "lu_uni_die:1",
        "frame_id": "frame_schools",
        "meaning_en": "a higher-education institution where people study and research, often offering degrees",
        "how_to_use_en": [
          "Use for the institution (often a named one): Uppsala universitet.",
          "Common with prepositions like vid/på to say where someone studies or works.",
          "Can be used in general: universitet eller högskola."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "studera vid [Institution]",
            "meaning_en": "study at a university",
            "note_en": "Used for where someone studies."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "professor i [Subject] vid [Institution]",
            "meaning_en": "professor in a subject at a university",
            "note_en": "Common job/role pattern in biographies."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "rektor för [Institution]",
            "meaning_en": "rector of a university",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "studenter på [Institution]",
            "meaning_en": "students at a university",
            "note_en": "Also appears as 'på universitet eller högskola'."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "från [Time] / under [Time] / åren [Time]",
            "meaning_en": "time period connected to university",
            "note_en": "Often used to say when someone studied or worked there."
          }
        ],
