        "sense_id": "lu_uni_die:1",
        "frame_id": "frame_education",
        "meaning_en": "a place for higher education and research, where people study and teach",
        "how_to_use_en": [
          "Use it for an institution of higher education (often with a place name).",
          "Common with prepositions like vid (“at”) and på (“at”).",
          "Can be singular or plural when talking about different universities."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "studera vid + [universitet]",
            "meaning_en": "study at a university",
            "note_en": "Used when naming the institution: vid Uppsala universitet."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "professor i + [ämne] vid + [universitet]",
            "meaning_en": "professor in a subject at a university",
            "note_en": ""
          },
          {
            "pattern_id": "pat_3",
            "pattern": "en institution vid + [universitet]",
            "meaning_en": "a department at a university",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "universitet eller högskola",
            "meaning_en": "university or college",
            "note_en": "A common pairing when speaking generally about higher education."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "från universitet till universitet",
            "meaning_en": "from one university to another",
            "note_en": "Repeating the noun shows movement between many places."
          }
        ],
