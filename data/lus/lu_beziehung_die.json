    "senses": [
      {
        "sense_id": "lu_beziehung_die:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "the way two people or things are connected, especially how they get along",
        "how_to_use_en": [
          "Use it for a personal connection: family, partners, friends.",
          "Also use it for connections between groups, countries, or ideas.",
          "Often combine with an adjective: god/komplicerad/nära relation."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "en (ADJ) relation till + person/sak",
            "meaning_en": "a (ADJ) relationship with/to",
            "note_en": "Common with family members: relation till sin far."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ha en (ADJ) relation (med) + någon",
            "meaning_en": "have a (ADJ) relationship",
            "note_en": "Often followed by a relative clause: med vilken han har en god relation."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "relation mellan + X och Y",
            "meaning_en": "relationship between two parties",
            "note_en": "Used for people, countries, or other entities."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "deras/systrarnas relation",
            "meaning_en": "their relationship",
            "note_en": "Possessive form highlights the pair/group."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "avsluta / träda in i + en relation",
            "meaning_en": "end / enter a relationship",
            "note_en": "Fixed verb + noun combinations in the examples."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *relation* only for romantic partners.",
            "fix": "Use *relation* for family, friends, and other connections too.",
            "explain_en": "In the examples it is used for father, grandfather, sisters, and also for countries/ideas."
          },
          {
            "error": "Saying *relation med* when you mean a person-to-person relationship.",
            "fix": "Prefer *relation till* or *relation mellan X och Y*.",
            "explain_en": "The examples show *relation till sin far* and *relation mellan man och kvinna*."
          }
