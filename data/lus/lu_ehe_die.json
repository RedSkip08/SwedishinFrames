        "meaning_en": "a legal relationship where two people are married",
        "how_to_use_en": [
          "Use for the institution/status of being married.",
          "Common with verbs like ingå (enter) and upplösa (dissolve).",
          "Often used with time expressions: år av sitt äktenskap."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "ingå (ett) äktenskap",
            "meaning_en": "enter into a marriage",
            "note_en": "Formal/legal style."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ingå äktenskap med + person",
            "meaning_en": "marry someone",
            "note_en": "Often used in biographies and legal texts."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "(sammanlagt) + antal + års äktenskap",
            "meaning_en": "years of marriage",
            "note_en": "Used to talk about duration."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "de första åren av sitt äktenskap",
            "meaning_en": "the first years of marriage",
            "note_en": ""
          },
          {
            "pattern_id": "pat_5",
            "pattern": "äktenskap + verb (havererar / upplöses)",
            "meaning_en": "a marriage breaks up",
            "note_en": "Common in narrative and formal writing."
          }
        ],
