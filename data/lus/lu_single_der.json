        "sense_id": "lu_single_der:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "a music single: one song released separately (often from an album).",
        "how_to_use_en": [
          "Used about a song: släppa/ge ut ... som singel.",
          "Often with an artist/group: en singel av X."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X är en singel av Y",
            "note_en": "X = song/title; Y = artist or band."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X släpptes som singel",
            "note_en": "Common passive pattern for release information."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "ge ut X som (en) singel",
            "note_en": "Active pattern: someone releases something as a single."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "en (första/tredje) singel",
            "note_en": "Often with an ordinal: first/third single."
          }
        ],
