    "senses": [
      {
        "sense_id": "lu_unterricht_der:1",
        "frame_id": "frame_education",
        "meaning_en": "organized teaching or instruction, often in a school or course setting",
        "how_to_use_en": [
          "Use for the activity of teaching (often in schools, courses, or training).",
          "Common with prepositions like i (in a subject) and på (at an institution).",
          "Can be used with verbs like bedriva, erbjuda, få, ge."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "undervisning i + Ämne",
            "meaning_en": "teaching in a subject",
            "note_en": "Used for fields, languages, or topics."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "undervisning på + Institution",
            "meaning_en": "teaching at an institution",
            "note_en": "Often with bedrivs/bedriva."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "få undervisning (av + Lärare)",
            "meaning_en": "receive teaching (from someone)",
            "note_en": "Focuses on the learner receiving instruction."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "erbjuda undervisning för + Studenter",
            "meaning_en": "offer teaching for learners",
            "note_en": "Often used about schools and grade levels."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "ge + Studenter + undervisning",
            "meaning_en": "give teaching to learners",
            "note_en": "Often paired with another noun like rådgivning."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *undervisning* when you mean the school as an institution.",
            "fix": "Use skola/lärosäte for the institution; use undervisning for the teaching activity.",
            "explain_en": "Undervisning is what happens (teaching), not the place/organization."
          },
          {
            "error": "Forgetting the preposition in subject expressions (e.g., *undervisning matematik*).",
            "fix": "Say undervisning i matematik.",
            "explain_en": "In the examples, the subject is introduced with i."
          }
