        "sense_id": "lu_unterricht_der:1",
        "frame_id": "frame_education",
        "meaning_en": "the activity of teaching, often in a school or course",
        "how_to_use_en": [
          "Use for teaching as an activity in schools, universities, or courses.",
          "Often appears with a subject (i) or with who receives it (för).",
          "Can be used with verbs like erbjuda, få, bedriva, ägna sig åt."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "undervisning i + ämne",
            "meaning_en": "teaching in a subject",
            "note_en": "Used for the field/subject taught."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "undervisning för + grupp",
            "meaning_en": "teaching for a group",
            "note_en": "Often for grades or a target group."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "få undervisning (hemma) (ett år)",
            "meaning_en": "receive teaching",
            "note_en": "Focuses on the learner receiving instruction."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "få undervisning av + lärare",
            "meaning_en": "be taught by someone",
            "note_en": "Names the teacher/instructor."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "bedriva/ägna sig åt + undervisning",
            "meaning_en": "carry out/do teaching",
            "note_en": "Common in formal texts, often with forskning."
          }
        ],
        "common_errors_en": [
