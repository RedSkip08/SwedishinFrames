        "sense_id": "lu_unterricht_der:1",
        "frame_id": "frame_classes_&_courses",
        "meaning_en": "organized teaching, often in a school or course, where someone learns a subject.",
        "how_to_use_en": [
          "Use for teaching as an activity or what a school/course provides.",
          "Often followed by i + subject, or av + instructor."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "undervisning i + Ämne",
            "meaning_en": "teaching in a subject",
            "note_en": "Used for the topic/field being taught."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "få undervisning av + Lärare",
            "meaning_en": "receive teaching from someone",
            "note_en": "Focuses on the instructor as source."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "erbjuda undervisning för + Elevgrupp",
            "meaning_en": "offer teaching for a group",
            "note_en": "Common with schools and grade levels."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "ha sin undervisning i + Plats",
            "meaning_en": "have teaching in a place",
            "note_en": "Often about where classes take place."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "starta sin undervisning + Tid",
            "meaning_en": "start teaching at a time",
            "note_en": "Used for institutions beginning teaching activities."
          }
        ],
