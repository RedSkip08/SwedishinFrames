        "sense_id": "lu_junggeselle_der:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "an unmarried man (often used as a social status description)",
        "how_to_use_en": [
          "Use after vara/bliva: vara ungkarl, bli ungkarl.",
          "Often used in biographies to describe someone’s marital status."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X vara ungkarl",
            "meaning_en": "X is a bachelor",
            "note_en": "Common in factual descriptions (e.g., biographies)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X (som länge) vara ungkarl",
            "meaning_en": "X has long been a bachelor",
            "note_en": "Often with time expressions like länge, fortfarande, vid den här tiden."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X leva som (en) ungkarl",
            "meaning_en": "X lives as a bachelor",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X:s tid som ungkarl",
            "meaning_en": "X’s time as a bachelor",
            "note_en": "Used when this period ends (e.g., after meeting someone)."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using ungkarl for any unmarried person.",
