        "sense_id": "lu_kämmen_die_haare_kämmen:1",
        "frame_id": "frame_grooming",
        "meaning_en": "use a comb to make hair neat or untangled.",
        "how_to_use_en": [
          "Use it for hair grooming: kamma håret.",
          "Often reflexive when you comb your own hair: kamma sig.",
          "With particle ut, it means to comb something out (e.g., dreadlocks, lice)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "kamma sig",
            "meaning_en": "comb your own hair",
            "note_en": "Reflexive: the person combs themself."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "kamma + håret",
            "meaning_en": "comb the hair",
            "note_en": "Body part as object."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "kamma + någon",
            "meaning_en": "comb someone (their hair)",
            "note_en": "The person is the object; hair is understood or can be added."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "kamma + någons hår",
            "meaning_en": "comb someone’s hair",
            "note_en": ""
          },
          {
            "pattern_id": "pat_5",
            "pattern": "kamma ut + något",
            "meaning_en": "comb something out",
            "note_en": "Used for removing/undoing, e.g. dreadlocks or lice."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *kamma* without an object when you mean hair.",
            "fix": "Say what you comb: kamma håret / kamma sig.",
            "explain_en": "In Swedish you usually mention the hair or use the reflexive pronoun."
          },
