      {
        "sense_id": "lu_vorlesung_die:1",
        "frame_id": "frame_education",
        "meaning_en": "a teaching session where someone speaks and others listen, often at a school or university",
        "how_to_use_en": [
          "Use for a planned talk in an educational setting.",
          "Often with hålla (“give”) and with a topic: om / i + subject."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "Vid en föreläsning ...",
            "meaning_en": "during a lecture",
            "note_en": "Vid marks what happens in that situation."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "hålla en föreläsning",
            "meaning_en": "give a lecture",
            "note_en": ""
          },
          {
            "pattern_id": "pat_3",
            "pattern": "hålla en föreläsning om + ämne",
            "meaning_en": "give a lecture about a topic",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "hålla en föreläsning i + ämne",
            "meaning_en": "give a lecture in a subject",
            "note_en": "Common with school/university subjects (e.g., matematik)."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "en föreläsning av + person",
            "meaning_en": "a lecture by someone",
            "note_en": ""
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *föreläsning* for any meeting or discussion.",
            "fix": "Use it for a talk where one person mainly speaks; otherwise use möte or diskussion.",
            "explain_en": "A föreläsning is typically one-way teaching/presenting, not a group meeting."
          },
          {
            "error": "Mixing up om and i for the topic.",
            "fix": "Use om + theme (om omdöme) and i + school subject (i matematik).",
            "explain_en": "Both introduce the topic, but i is common with academic subjects."
          }
        ],
