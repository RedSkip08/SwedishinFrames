      {
        "sense_id": "lu_verhältnis_das:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "a personal relationship between people, often romantic or within a family",
        "how_to_use_en": [
          "Use about two people (or a group) and how they relate to each other.",
          "Common with verbs like ha, inleda, leva i, avsluta, and with adjectives like stormigt, instabilt, spänt.",
          "Often followed by a preposition phrase: med + person or till + person."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "ha ett (ADJ) förhållande (med X)",
            "note_en": "E.g. 'hade ett instabilt förhållande ...', 'har ett förhållande med ...'."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ha ett (ADJ) förhållande till X",
            "note_en": "Often for family/social relations: 'ett dåligt förhållande till modern'."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "leva i ett (ADJ) förhållande med X",
            "note_en": "Describes being in a relationship for a period: 'levde hon i ett stormigt förhållande med ...'."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "inleda ett förhållande",
            "note_en": "To start a relationship: 'De inleder ett förhållande'."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "avsluta sitt förhållande",
            "note_en": "To end a relationship: 'avslutade sitt förhållande'."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *förhållande* for a mathematical ratio or general situation in these patterns.",
            "fix": "Use it for personal relations here; for other meanings, Swedish often uses other patterns/words.",
            "explain_en": "In the given examples, förhållande is about people’s relationship (not numbers or conditions)."
          }
