        "meaning_en": "the direction a vehicle, person, or natural force moves in",
        "how_to_use_en": [
          "Use about travel/navigation: ships, vehicles, convoys, storms, rivers.",
          "Often with verbs like sätta, ha, hålla, behålla and with mot + place."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "ha kurs + (väderstreck)",
            "meaning_en": "head in a direction",
            "note_en": "Often with a compass direction like norrut."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "sätta kurs mot + plats",
            "meaning_en": "set course toward a place",
            "note_en": ""
          },
          {
            "pattern_id": "pat_3",
            "pattern": "med kurs mot + plats",
            "meaning_en": "with a course toward a place",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "hålla + (adjektiv) kurs",
            "meaning_en": "keep a (straight) course",
            "note_en": "Often with rak or a compass adjective like västnordvästlig."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "behålla + X:s kurs",
            "meaning_en": "maintain someone’s/its course",
            "note_en": "Common with vehicles: fordonets kurs."
          }
        ],
