    "senses": [
      {
        "sense_id": "lu_kurs_der:1",
        "frame_id": "frame_classes_&_courses",
        "meaning_en": "the direction or route something moves or is aimed toward",
        "how_to_use_en": [
          "Use for travel/movement direction: ships, vehicles, storms, rivers.",
          "Often with verbs like sätta, hålla, ha and with mot + place."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "sätta kurs mot + Plats",
            "meaning_en": "set a course toward",
            "note_en": "Common for ships/convoys/lifeboats."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ha kurs + Riktning (t.ex. norrut)",
            "meaning_en": "head in a direction",
            "note_en": "Often followed by a later change of direction."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "hålla + Adjektiv + kurs",
            "meaning_en": "keep a (stable) course",
            "note_en": "Used for storms or currencies (metaphorically)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X:s kurs",
            "meaning_en": "X’s course/path",
            "note_en": "Used for rivers or vehicles: flodens/fordonets kurs."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "med kurs mot + Plats",
            "meaning_en": "with a course toward",
            "note_en": "Often after verbs like lämna/avsegla."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using kurs to mean a school class in these patterns (e.g., *sätta kurs i svenska*).",
            "fix": "Use kurs for direction here; use kurs i svenska for a class/course.",
            "explain_en": "The examples show navigation/route meaning, not education."
          },
          {
            "error": "Saying *kurs till* instead of kurs mot.",
            "fix": "Say kurs mot + destination.",
            "explain_en": "In the examples, the destination is introduced with mot."
          }
