      {
        "sense_id": "lu_seminar_das:1",
        "frame_id": "frame_education",
        "meaning_en": "a planned meeting or class where people discuss a topic and share knowledge",
        "how_to_use_en": [
          "Use for an organized event about a specific subject, often at an institution.",
          "Common with verbs like anordna, arrangera, genomföra, hålla.",
          "Often followed by om + topic, or i + place/institution."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "ett seminarium om + Ämne",
            "meaning_en": "a seminar about a topic",
            "note_en": "Use om to name what the seminar is about."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ett + adj + seminarium",
            "meaning_en": "a seminar of a certain type",
            "note_en": "Often describes the subject area or level (e.g., skattepolitiskt, högre)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "seminarium i + Plats/Institution",
            "meaning_en": "a seminar in a place/institution",
            "note_en": "Used for location, e.g., i Riksdagen, i Stockholm."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "arrangera/anordna/genomföra/hålla + (ett) seminarium",
            "meaning_en": "organize or hold a seminar",
            "note_en": "The organizer is often an institution."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "inom ramen för arbetet i (ett) seminarium",
            "meaning_en": "within the work in a seminar",
            "note_en": "Common in academic contexts."
          }
        ],
