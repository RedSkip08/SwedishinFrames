    "senses": [
      {
        "sense_id": "lu_seminar_das:1",
        "frame_id": "frame_education",
        "meaning_en": "a planned meeting for learning and discussion, often arranged by an organization or institution",
        "how_to_use_en": [
          "Use it for an organized learning/discussion event: ett seminarium.",
          "Often add topic with om + subject.",
          "Often mention organizer with arrangerat av / anordnat av."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "anordna/ordna/genomföra ett seminarium",
            "meaning_en": "arrange or hold a seminar",
            "note_en": "Used when an institution or group is the organizer."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "ett seminarium i + plats",
            "meaning_en": "a seminar in a place",
            "note_en": "Often used for venues like Riksdagen."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "ett seminarium om + ämne",
            "meaning_en": "a seminar about a topic",
            "note_en": "Common for political, social, or academic topics."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "ett + adj + seminarium",
            "meaning_en": "a seminar of a certain type",
            "note_en": "Examples include längre, högre, skattepolitiskt."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "delta i ett seminarium (med + person)",
            "meaning_en": "take part in a seminar",
            "note_en": "Med can name the teacher/leader or another participant."
          }
        ],
        "common_errors_en": [
