      {
        "sense_id": "lu_klasse_die:1",
        "frame_id": "frame_education",
        "meaning_en": "a group of pupils who study together in school, often named by grade and letter.",
        "how_to_use_en": [
          "Use for a school group (e.g., klass 6A) or a grade level (e.g., femte klass).",
          "Often used with a number/letter or with an adjective (vanlig/öppen).",
          "Can be linked to a school: På skolan finns en klass per årskurs."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "en klass per årskurs",
            "meaning_en": "one class for each grade",
            "note_en": "Used when describing how a school is organized."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "från femte klass",
            "meaning_en": "from fifth grade",
            "note_en": "Here klass refers to grade level."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "i klass 1 B / klass 6A",
            "meaning_en": "in class 1B/6A",
            "note_en": "Number + letter commonly names a specific class group."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "gå i en vanlig klass",
            "meaning_en": "attend a regular class",
            "note_en": "Used to contrast with special arrangements."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "en öppen klass (för ...)",
            "meaning_en": "an open/special class (for ...)",
            "note_en": "Often followed by a phrase describing the pupil group."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using klass for a single lesson (e.g., “a math class” as one session).",
            "fix": "Use lektion (one lesson) or kurs (a course) instead.",
            "explain_en": "In these examples, klass means a pupil group or grade level, not one meeting."
          },
          {
            "error": "Mixing up klass (group) and årskurs (grade/year).",
            "fix": "Use årskurs for the school year level; use klass for the group (often with a letter).",
            "explain_en": "You can have one klass per årskurs, and classes can be merged across årskurser."
          }
        ],
