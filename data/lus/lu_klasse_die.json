      {
        "sense_id": "lu_klasse_die:1",
        "frame_id": "frame_education",
        "meaning_en": "a group of students in a school, or a school grade level",
        "how_to_use_en": [
          "Use for a school group: en klass, klass 6A.",
          "Use with an ordinal for grade level: femte klass.",
          "Often mentioned together with a school: på skolan finns en klass per årskurs."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "en klass (per årskurs)",
            "meaning_en": "a class group",
            "note_en": "Often used when describing how a school is organized."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "i klass + number + letter (t.ex. klass 6A, klass 1 B)",
            "meaning_en": "in a specific class",
            "note_en": "Common school label with number and sometimes a letter."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "ordinal + klass (t.ex. femte klass)",
            "meaning_en": "a grade level",
            "note_en": "Refers to the school year level, not the group name."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "en vanlig/öppen klass",
            "meaning_en": "a type of class group",
            "note_en": "An adjective can describe what kind of class it is."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using klass for a single lesson (like English “class” = lesson).",
            "fix": "Use lektion (or kurs) when you mean a lesson/course.",
            "explain_en": "In these examples, klass means a school group or grade level, not one lesson."
          },
          {
            "error": "Saying *i skolan* when you mean the class group.",
            "fix": "Say i en klass / i klass 6A.",
            "explain_en": "Use i + klass to place someone in a specific class group."
          }
        ],
