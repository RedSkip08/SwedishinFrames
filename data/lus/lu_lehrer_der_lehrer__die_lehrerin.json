    "senses": [
      {
        "sense_id": "lu_lehrer_der_lehrer__die_lehrerin:1",
        "frame_id": "frame_education",
        "meaning_en": "a person who teaches students a subject or skill, often in a school or other institution",
        "how_to_use_en": [
          "Use for someone whose job is to teach (in a school or similar).",
          "Often followed by what they teach (i + subject/skill) or where they work (vid + institution).",
          "Can also mean a private teacher for a skill (e.g., music)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X:s lärare",
            "meaning_en": "someone’s teacher",
            "note_en": "Genitive shows who the teacher teaches (e.g., Griegs lärare)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "utbilda sig till lärare",
            "meaning_en": "train to become a teacher",
            "note_en": ""
          },
          {
            "pattern_id": "pat_3",
            "pattern": "arbeta som lärare (vid + institution)",
            "meaning_en": "work as a teacher",
            "note_en": "Often adds workplace with vid."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "vara lärare i + ämne",
            "meaning_en": "teach a subject",
            "note_en": "Uses i before the subject area (e.g., i sång)."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "lärare som + verb",
            "meaning_en": "teachers who do something",
            "note_en": "Common for describing what teachers provide (e.g., give bilingual teaching)."
          }
        ],
        "common_errors_en": [
