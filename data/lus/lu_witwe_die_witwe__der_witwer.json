        "frame_id": "frame_personal_relationship",
        "meaning_en": "a woman whose spouse has died",
        "how_to_use_en": [
          "Use about a woman after her spouse has died.",
          "Often used with a possessive or a name: hans änka, X:s änka.",
          "Also used after the verb bli: bli änka."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X:s änka",
            "meaning_en": "X’s widow",
            "note_en": "Used to identify the widow by the deceased spouse."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "hans änka",
            "meaning_en": "his widow",
            "note_en": "Possessive pronoun + änka is common in narratives."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bli änka (årtal)",
            "meaning_en": "become a widow",
            "note_en": "Often followed by a year in biographies."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "bli änka och gifta om sig",
            "meaning_en": "be widowed and remarry",
            "note_en": "Common in life-story descriptions."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using änka for a man.",
            "fix": "Use änkling for a man.",
            "explain_en": "Änka is female; the male word is änkling."
          },
          {
            "error": "Saying *vara änka* when the context is the change of status.",
            "fix": "Use bli änka when you mean 'become a widow'.",
            "explain_en": "The examples show bli änka for the event of becoming a widow."
          }
