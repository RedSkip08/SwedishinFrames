        "sense_id": "lu_witwe_die_witwe__der_witwer:1",
        "frame_id": "frame_marriage",
        "meaning_en": "a woman whose spouse has died",
        "how_to_use_en": [
          "Use as a noun for a woman after her husband has died.",
          "Often used with vara (“är änka”) or bli (“blev/har blivit änka”).",
          "Can be used with a possessive: hans änka (“his widow”)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "en + (adjektiv) + änka",
            "meaning_en": "a (adjective) widow",
            "note_en": "Used to describe the widow (e.g., fattig)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X är änka",
            "meaning_en": "X is a widow",
            "note_en": "States current status."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X blev/har blivit änka",
            "meaning_en": "X became a widow",
            "note_en": "Focuses on the change of status."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "hans änka",
            "meaning_en": "his widow",
            "note_en": "Possessive refers to the deceased husband."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "en änka vars man ...",
            "meaning_en": "a widow whose husband ...",
            "note_en": "Often followed by information about the husband’s death."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using änka for any person whose spouse has died.",
            "fix": "Use änka for a woman; use änkling for a man.",
            "explain_en": "The word änka is gender-specific in Swedish."
          },
          {
            "error": "Saying *är änka* when you mean divorced.",
            "fix": "Use skild (from skilja sig) for divorced.",
            "explain_en": "Änka implies the spouse died, not separation or divorce."
          }
        ],
