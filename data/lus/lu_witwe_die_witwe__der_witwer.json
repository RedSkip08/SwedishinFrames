    "senses": [
      {
        "sense_id": "lu_witwe_die_witwe__der_witwer:1",
        "frame_id": "frame_marriage",
        "meaning_en": "a woman whose husband has died; her marriage has ended because of death",
        "how_to_use_en": [
          "Use as a noun for a woman after her spouse dies.",
          "Often used with bli: bli änka, and with time words (years).",
          "Can be used as a status: vara änka."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "bli änka",
            "note_en": "Very common way to express the change of status."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "bli änka + Time (årtal)",
            "note_en": "Often followed by a year: e.g., 1932, 1547, 1699, 1956."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "vara änka",
            "note_en": "Describes current status (not the change)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X:s änka",
            "note_en": "Possessive: ‘his widow’ (Hans änka)."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using änka for a man.",
            "fix": "Use änkling for a man.",
            "explain_en": "Änka is female; the male word is änkling."
          },
          {
            "error": "Confusing änka with divorced (skild).",
            "fix": "Use skild/skilsmässa for divorce; änka is only when the spouse has died.",
            "explain_en": "In the examples, the status comes after the spouse’s death, not after divorce."
          }
        ],
