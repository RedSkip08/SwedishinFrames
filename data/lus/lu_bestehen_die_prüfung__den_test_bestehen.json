        "frame_id": "frame_education",
        "meaning_en": "be able to do something successfully; pass a test or meet a requirement",
        "how_to_use_en": [
          "Use with a direct object: klara + test/level/requirement/challenge.",
          "Often means “manage/handle” with difficult conditions, weight, heat, illness.",
          "Can mean “pass” with prov/test."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "klara + prov/test",
            "meaning_en": "pass an exam/test",
            "note_en": "Used for formal or practical tests (e.g., det teoretiska provet)."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "klara + krav/standard",
            "meaning_en": "meet a requirement/standard",
            "note_en": "Common with rules and technical standards (e.g., avgaskrav, Euro 6)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "klara + belastning (tyngd/hetta/transport)",
            "meaning_en": "withstand a load/conditions",
            "note_en": "Often about machines, buildings, or materials."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "klara + sjukdom/utmaning",
            "meaning_en": "cope with illness/challenges",
            "note_en": "Focuses on getting through something difficult."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "klara + banor",
            "meaning_en": "complete levels/stages",
            "note_en": "Common in games and tasks with stages."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *klara* without an object when you mean “manage something”.",
            "fix": "Add what is managed: klara + noun (e.g., klara provet).",
            "explain_en": "In these examples, klara is transitive: you manage/pass something."
          },
