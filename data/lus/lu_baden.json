      {
        "sense_id": "lu_baden:1",
        "frame_id": "frame_grooming",
        "meaning_en": "wash yourself in water, or go into water to swim for pleasure.",
        "how_to_use_en": [
          "Use for bathing in a bathtub or bathing/swimming outdoors.",
          "Often used without an object: bada.",
          "Use a place phrase to say where you bathe (e.g., i sjön, på stranden)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "bada",
            "meaning_en": "bathe; go swimming",
            "note_en": "Often used generally, without saying where."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "bada i + place",
            "meaning_en": "bathe in a place",
            "note_en": "Used for lakes, waterfalls, and other water locations."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "bada från + place",
            "meaning_en": "bathe from a spot",
            "note_en": "Says the starting point, e.g., from rocks."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "gå till + place + för att bada",
            "meaning_en": "go somewhere to bathe",
            "note_en": "Purpose expressed with för att + infinitive."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "bada i + container (badkar)",
            "meaning_en": "bathe in a bathtub",
            "note_en": "Used for bathing indoors."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using bada only for swimming and never for washing in a bathtub.",
            "fix": "Use bada for both: bathing in a bathtub and bathing/swimming outdoors.",
            "explain_en": "In Swedish, bada covers both meanings in everyday use."
          },
          {
            "error": "Forgetting the preposition with places (e.g., *bada stranden).",
            "fix": "Say bada på stranden / bada i sjön.",
            "explain_en": "Places are typically expressed with a prepositional phrase."
          }
        ],
