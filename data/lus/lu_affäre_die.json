    "senses": [
      {
        "sense_id": "lu_affäre_die:1",
        "frame_id": "frame_personal_relationship",
        "meaning_en": "a romantic relationship, often short and sometimes secret",
        "how_to_use_en": [
          "Use about two people who are romantically involved: ha en kärleksaffär.",
          "Often with adjectives like kort, passionerad, hemlig.",
          "Often with verbs like inleda: inleda en kärleksaffär (med någon)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X har en (kort/hemlig/passionerad) kärleksaffär",
            "note_en": "Describes that someone is in a love affair."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X har en kärleksaffär med Y",
            "note_en": "Names the other partner with med."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X inleder en (kort) kärleksaffär med Y",
            "note_en": "Focus on starting the affair."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "de/paret/de två har/inleder en kärleksaffär",
            "note_en": "Partners expressed as a plural subject."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "det antyds/misstänks att X har en kärleksaffär",
            "note_en": "Used when it is only suggested or suspected."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using kärleksaffär for any long-term relationship.",
            "fix": "Use förhållande/relation for neutral long-term relationships.",
            "explain_en": "kärleksaffär often sounds like a (possibly secret) affair, not just a normal relationship."
          },
          {
            "error": "Forgetting the preposition with a partner: *inleda en kärleksaffär henne.",
            "fix": "Say: inleda en kärleksaffär med henne.",
            "explain_en": "With a named partner, Swedish commonly uses med."
          }
        ],
