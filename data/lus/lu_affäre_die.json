      {
        "sense_id": "lu_affäre_die:1",
        "frame_id": "frame_marriage",
        "meaning_en": "a secret romantic or sexual relationship that breaks a committed relationship, often marriage.",
        "how_to_use_en": [
          "Used about someone having a relationship outside their partner relationship.",
          "Often appears with verbs like ha, inleda, avslöja, upptäcka.",
          "Common with time/reason phrases: efter, när, i juli."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "ha en otrohetsaffär (med + person)",
            "meaning_en": "have an affair (with someone)",
            "note_en": "Often used when someone is already married or partnered."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "inleda en otrohetsaffär (med + person)",
            "meaning_en": "start an affair",
            "note_en": ""
          },
          {
            "pattern_id": "pat_3",
            "pattern": "efter en (påstådd) otrohetsaffär",
            "meaning_en": "after an (alleged) affair",
            "note_en": "Common as a reason for separation/divorce."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "upptäcka/misstänka att X har en otrohetsaffär",
            "meaning_en": "discover/suspect an affair",
            "note_en": ""
          },
          {
            "pattern_id": "pat_5",
            "pattern": "en otrohetsaffär avslöjas",
            "meaning_en": "an affair is revealed",
            "note_en": ""
          }
        ],
        "common_errors_en": [
          {
            "error": "Using affär for this meaning in all contexts.",
            "fix": "Use otrohetsaffär when you mean cheating; affär often means business/shop.",
