    "senses": [
      {
        "sense_id": "lu_freundin_die:1",
        "frame_id": "frame_dating",
        "meaning_en": "a female romantic partner in a relationship (not married).",
        "how_to_use_en": [
          "Use for someone’s female partner: en/min/sin/hans flickvän.",
          "Often followed by a name: sin flickvän + Name.",
          "Can be used with relationship words like vara tillsammans."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "X har en flickvän",
            "meaning_en": "X has a girlfriend",
            "note_en": "Often with time adverbs like redan."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "X med sin flickvän (Namn)",
            "meaning_en": "X with their girlfriend",
            "note_en": "Common in biographies and descriptions."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X har varit tillsammans med sin flickvän (sedan + tid)",
            "meaning_en": "X has been together with girlfriend",
            "note_en": "Used to say how long the relationship has lasted."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "X gifte sig med sin flickvän",
            "meaning_en": "X married their girlfriend",
            "note_en": "Describes a change of relationship status."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "X var flickvän till Y",
            "meaning_en": "X was Y’s girlfriend",
            "note_en": "More formal; often used in written texts."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using flickvän for any female friend.",
            "fix": "Use vän or kompis for a non-romantic friend.",
            "explain_en": "flickvän normally means a romantic partner, not just a friend."
          },
          {
            "error": "Mixing up sin and hans/hennes.",
            "fix": "Use sin when the subject owns the girlfriend; otherwise hans/hennes.",
            "explain_en": "Swedish uses reflexive possessives (sin/sitt/sina) when it refers back to the subject."
          }
        ],
