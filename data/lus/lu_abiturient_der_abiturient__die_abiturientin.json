    "senses": [
      {
        "sense_id": "lu_abiturient_der_abiturient__die_abiturientin:1",
        "frame_id": "frame_education",
        "meaning_en": "a person who studies, especially at a university",
        "how_to_use_en": [
          "Use for someone enrolled at a university/higher education institution.",
          "Often used with an institution: \"student vid\" + university.",
          "In biographies, \"blev student\" can mean started university studies or qualified for them."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "bli student vid + Institution",
            "meaning_en": "become a student at an institution",
            "note_en": "Common in biographies with a year."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "bli student i + Place",
            "meaning_en": "become a student in a place",
            "note_en": "Used when the city is mentioned rather than the institution."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "student vid + Institution",
            "meaning_en": "student at an institution",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "student från + Institution",
            "meaning_en": "student from a school",
            "note_en": "Can refer to finishing/qualifying from a school (older usage)."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using student for any school pupil in all contexts.",
            "fix": "Use elev for a pupil; use student mainly for university/higher education.",
            "explain_en": "In Swedish, student often means someone at university, while elev is common for school."
          },
          {
            "error": "Saying *student på Uppsala universitet in this sense.",
            "fix": "Say student vid Uppsala universitet.",
            "explain_en": "With institutions, vid is the most typical preposition in the examples."
          }
        ],
