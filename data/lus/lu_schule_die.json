        "meaning_en": "an educational institution or school building where teaching and learning happen",
        "how_to_use_en": [
          "Use for a school as an institution or a physical place.",
          "Often used with a place phrase: i + city/area.",
          "Can be part of a school name: X skola."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "en skola",
            "meaning_en": "a (non-specified) school",
            "note_en": "Used when introducing a school for the first time."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "skola i + plats",
            "meaning_en": "a school in a place",
            "note_en": "Location is commonly added with i + place name."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "X skola",
            "meaning_en": "a named school",
            "note_en": "Common in proper names (e.g., a local school name)."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "skola, + (other institutions)",
            "meaning_en": "school as one facility among others",
            "note_en": "Often listed with other local services (e.g., shop, nursing home)."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "rätt till + skola",
            "meaning_en": "right to schooling",
            "note_en": "Used in policy/rights contexts."
          }
        ],
