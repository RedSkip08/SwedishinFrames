        "frame_id": "frame_education",
        "meaning_en": "one year of studies in a program, course, or at an institution",
        "how_to_use_en": [
          "Use for a specific year in a study program (first, second, fourth).",
          "Often used with ordinal numbers and possessives: sitt första/fjärde studieår.",
          "Can also mean the period someone spent studying (often plural: sina studieår)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "efter sitt [ordinal] studieår",
            "meaning_en": "after a certain study year",
            "note_en": "Used to place an event after a specific year in a program."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "under sitt [ordinal] studieår",
            "meaning_en": "during a certain study year",
            "note_en": "Common in biographies and narratives."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "under sina studieår",
            "meaning_en": "during one’s study years",
            "note_en": "Usually plural, meaning the whole period of studying."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "studieår vid [institution]",
            "meaning_en": "study years at an institution",
            "note_en": "Often used with \"vid\" to name where someone studied."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "söka ett [ordinal] studieår",
            "meaning_en": "apply for another study year",
            "note_en": "Used when a program offers an additional year."
          }
        ],
        "common_errors_en": [
