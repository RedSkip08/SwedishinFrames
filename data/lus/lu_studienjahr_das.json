        "sense_id": "lu_studienjahr_das:1",
        "frame_id": "frame_education",
        "meaning_en": "a year (or years) spent studying in an education program",
        "how_to_use_en": [
          "Use for one specific year (första/andra/fjärde studieår) or for several years (studieår).",
          "Often used with under/efter + studieår, and can be linked to a course or institution."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "efter sitt/ sitt + ordinal + studieår",
            "meaning_en": "after a certain study year",
            "note_en": "Used when something happens after completing that year."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "under sina/sitt + studieår",
            "meaning_en": "during one’s years of study",
            "note_en": "Often refers to a longer period (plural) in education."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "efter studieår vid/på + Institution",
            "meaning_en": "after studying at an institution",
            "note_en": "Common in biographies to summarize a study period."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "ta/söka ett + (extra/andra) + studieår",
            "meaning_en": "take/apply for a study year",
            "note_en": "Used for adding or continuing a program with another year."
          }
        ],
