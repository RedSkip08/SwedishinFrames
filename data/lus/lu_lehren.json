      {
        "sense_id": "lu_lehren:1",
        "frame_id": "frame_education",
        "meaning_en": "to teach someone a subject or skill, often in a school or course setting",
        "how_to_use_en": [
          "Use for teaching a subject/skill, often formally (school, course, institution).",
          "Common with i + subject and vid/på + institution.",
          "Can be used without an object when the activity is the focus."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "undervisa i + Ämne",
            "meaning_en": "teach a subject",
            "note_en": "Used for school subjects and academic fields."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "undervisa + Elev/Person",
            "meaning_en": "teach someone",
            "note_en": "Direct object is the learner (e.g., a person or group)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "undervisa + Elev/Person + i + Ämne",
            "meaning_en": "teach someone a subject",
            "note_en": ""
          },
          {
            "pattern_id": "pat_4",
            "pattern": "undervisa vid/på + Institution",
            "meaning_en": "teach at an institution",
            "note_en": "Both vid and på occur with schools/colleges."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "undervisa + (andra/barn/befolkningen) + Hur/att + Färdighet",
            "meaning_en": "teach how to do something",
            "note_en": "Often introduces a skill with hur + clause."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *undervisa* when you mean “learn” (lära sig).",
            "fix": "Use lära sig for learning; use undervisa for teaching.",
            "explain_en": "Undervisa focuses on the teacher’s action, not the student’s learning."
          },
