    "senses": [
      {
        "sense_id": "lu_lehren:1",
        "frame_id": "frame_teaching",
        "meaning_en": "to teach someone a subject or skill, often in a school or organized setting",
        "how_to_use_en": [
          "Use for giving instruction in a subject (often with i + subject).",
          "Often used about teaching at a school or institution (vid/på + place).",
          "Can be used generally: undervisa (without saying what)."
        ],
        "patterns": [
          {
            "pattern_id": "pat_1",
            "pattern": "undervisa i + ämne",
            "meaning_en": "teach a subject",
            "note_en": "Common with school subjects and fields."
          },
          {
            "pattern_id": "pat_2",
            "pattern": "undervisa + någon + i + ämne",
            "meaning_en": "teach someone something",
            "note_en": "The learner is an object (e.g., dem, barn)."
          },
          {
            "pattern_id": "pat_3",
            "pattern": "undervisa vid + institution",
            "meaning_en": "teach at an institution",
            "note_en": "Used with universities/colleges in the examples."
          },
          {
            "pattern_id": "pat_4",
            "pattern": "undervisa på + skolor",
            "meaning_en": "teach at schools",
            "note_en": "Often with types of schools (e.g., folkskolor)."
          },
          {
            "pattern_id": "pat_5",
            "pattern": "börja undervisa",
            "meaning_en": "start teaching",
            "note_en": "Very common in biographical contexts with a time expression."
          }
        ],
        "common_errors_en": [
          {
            "error": "Using *undervisa* for 'learn' (studying yourself).",
            "fix": "Use lära sig or studera for 'learn/study'.",
            "explain_en": "Undervisa focuses on the teacher giving instruction, not the learner."
          },
          {
            "error": "Saying *undervisa tyska* without i.",
            "fix": "Say undervisa i tyska.",
            "explain_en": "In the examples, the subject is introduced with i."
          }
